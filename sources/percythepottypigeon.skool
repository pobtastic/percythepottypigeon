 $6B81 DEC (HL)        ; Decrement *#REGhl.
 $6B82 RET NZ          ; Return if the delay hasn't expired yet.
; Delay expired; determine which direction the red bird should enter from
; based on which room it's coming from relative to Percy's room.
 $6B83 XOR A           ; {Write #N$00 to *#R$6CB3 (default: flying left).
 $6B84 LD ($6CB3),A    ; }
 $6B87 LD A,($5FC5)    ; {#REGb=*#R$5FC5.
 $6B8A LD B,A          ; }
 $6B8B LD A,($6CB6)    ; #REGa=*#R$6CB6.
 $6B8E CP B            ; {Jump to #R$6B96 if the red bird is coming from a
 $6B8F JR C,$6B96      ; lower numbered room.}
; Red bird is coming from a higher room so enter flying right.
 $6B91 LD A,$01        ; {Write #N$01 to *#R$6CB3 (flying right).
 $6B93 LD ($6CB3),A    ; }
@label=UpdateRedBirdMovement_EnterRoom
*$6B96 CALL $6AA1      ; Call #R$6AA1.
 $6B99 JR $6B5F        ; Jump to #R$6B5F.
; Handle the red bird's stun recovery. The stun timer counts down, and
; the red bird flashes blue while stunned. When the timer expires, the red bird
; becomes active again with a short appearance delay.
@label=UpdateRedBirdMovement_StunRecovery
*$6B9B LD HL,$6CB4     ; #REGhl=#R$6CB4 (stun timer).
 $6B9E LD A,$01        ; {Write #INK$01 to *#R$DAC6.
 $6BA0 LD ($DAC6),A    ; }
 $6BA3 DEC (HL)        ; Decrement the stun timer.
 $6BA4 LD A,(HL)       ; #REGa=*#REGhl.
 $6BA5 CP $01          ; {Jump to #R$6BC0 if the stun is about to end.
 $6BA7 JR Z,$6BC0      ; }
; Still stunned so only draw if the red bird is in the current room.
 $6BA9 LD A,($6CB6)    ; {#REGb=*#R$6CB6.
 $6BAC LD B,A          ; }
 $6BAD LD A,($5FC5)    ; #REGa=*#R$5FC5.
 $6BB0 CP B            ; {Return if the red bird is not in the same room as
 $6BB1 RET NZ          ; Percy.}
; Red bird is stunned and visible so move it downward slowly (falling).
 $6BB2 LD C,(IX+$00)   ; #REGc=*#REGix+#N$00 (red bird's X position).
 $6BB5 LD A,(IX+$01)   ; #REGa=*#REGix+#N$01 (red bird's Y position).
 $6BB8 ADD A,$03       ; #REGa+=#N$03 (drift downward).
 $6BBA LD B,A          ; Store the result in #REGb.
 $6BBB CALL $6C0C      ; Call #R$6C0C.
 $6BBE JR $6B5F        ; Jump to #R$6B5F.
; Stun ending so clear the timer and set a short reappearance delay.
@label=UpdateRedBirdMovement_StunEnd
*$6BC0 DEC (HL)        ; Clear the stun timer to #N$00.
; Set a short reappearance delay.
 $6BC1 LD A,$04        ; {Write #N$04 to *#R$6CBD.
 $6BC3 LD ($6CBD),A    ; }
 $6BC6 JR $6B6E        ; Jump to #R$6B6E.

; Red Bird Direction Jump Table
;
; Used by the routines at #R$6AF6, #R$7439 and #R$7512.
; .
; Movement direction jump table. The entry point is self-modified at
; #R$6BC8(#N$6BC9) to jump to one of 8 directional movement handlers. Each
; handler adjusts #REGb (Y) and/or #REGc (X) by the flight speed value at
; *#REGhl, then falls through to #R$6C0C to validate the position.
; #TABLE(default,centre,centre,centre,centre)
; { =h Direction | =h Index | =h X | =h Y }
; { Up | 0 | - | speed }
; { Up-right | 1 | +speed | speed }
; { Right | 2 | +speed | - }
; { Down-right | 3 | +speed | +speed }
; { Down | 4 | - | +speed }
; { Down-left | 5 | speed | +speed }
; { Left | 6 | speed | - }
; { Up-left | 7 | speed | speed }
; TABLE#
@label=RedBirdDirectionJumpTable
c$6BC8 JR $6BCA      ; Self-modified jump; offset written by #R$6B4E.
@label=RedBirdDirection_Up
*$6BCA JR $6BDA      ; Jump to #R$6BDA (up).
@label=RedBirdDirection_UpRight
 $6BCC JR $6BDF      ; Jump to #R$6BDF (up-right).
@label=RedBirdDirection_Right
 $6BCE JR $6BE7      ; Jump to #R$6BE7 (right).
@label=RedBirdDirection_DownRight
 $6BD0 JR $6BEC      ; Jump to #R$6BEC (down-right).
@label=RedBirdDirection_Down
 $6BD2 JR $6BF4      ; Jump to #R$6BF4 (down).
@label=RedBirdDirection_DownLeft
 $6BD4 JR $6BF9      ; Jump to #R$6BF9 (down-left).
@label=RedBirdDirection_Left
 $6BD6 JR $6C01      ; Jump to #R$6C01 (left).
@label=RedBirdDirection_UpLeft
 $6BD8 JR $6C06      ; Jump to #R$6C06 (up-left).
; Movement handlers. Each adjusts #REGb and/or #REGc by the speed
; value at *#REGhl, then falls through to #R$6C0C.
@label=RedBirdMove_Up
*$6BDA LD A,B        ; {#REGb-=*#REGhl (subtract speed from Y).
 $6BDB SUB (HL)      ;
 $6BDC LD B,A        ; }
 $6BDD JR $6C0C      ; Jump to #R$6C0C.
@label=RedBirdMove_UpRight
*$6BDF LD A,B        ; {#REGb-=*#REGhl (Y  speed).
 $6BE0 SUB (HL)      ;
 $6BE1 LD B,A        ; }
 $6BE2 LD A,C        ; {#REGc+=*#REGhl (X + speed).
 $6BE3 ADD A,(HL)    ;
 $6BE4 LD C,A        ; }
 $6BE5 JR $6C0C      ; Jump to #R$6C0C.
@label=RedBirdMove_Right
*$6BE7 LD A,C        ; {#REGc+=*#REGhl (X + speed).
 $6BE8 ADD A,(HL)    ;
 $6BE9 LD C,A        ; }
 $6BEA JR $6C0C      ; Jump to #R$6C0C.
@label=RedBirdMove_DownRight
*$6BEC LD A,C        ; {#REGc+=*#REGhl (X + speed).
 $6BED ADD A,(HL)    ;
 $6BEE LD C,A        ; }
 $6BEF LD A,B        ; {#REGb+=*#REGhl (Y + speed).
 $6BF0 ADD A,(HL)    ;
 $6BF1 LD B,A        ; }
 $6BF2 JR $6C0C      ; Jump to #R$6C0C.
@label=RedBirdMove_Down
*$6BF4 LD A,B        ; {#REGb+=*#REGhl (Y + speed).
 $6BF5 ADD A,(HL)    ;
 $6BF6 LD B,A        ; }
 $6BF7 JR $6C0C      ; Jump to #R$6C0C.
@label=RedBirdMove_DownLeft
*$6BF9 LD A,B        ; {#REGb+=*#REGhl (Y + speed).
 $6BFA ADD A,(HL)    ;
 $6BFB LD B,A        ; }
 $6BFC LD A,C        ; {#REGc-=*#REGhl (X  speed).
 $6BFD SUB (HL)      ;
 $6BFE LD C,A        ; }
 $6BFF JR $6C0C      ; Jump to #R$6C0C.
@label=RedBirdMove_Left
*$6C01 LD A,C        ; {#REGc-=*#REGhl (X  speed).
 $6C02 SUB (HL)      ;
 $6C03 LD C,A        ; }
 $6C04 JR $6C0C      ; Jump to #R$6C0C.
@label=RedBirdMove_UpLeft
*$6C06 LD A,B        ; {#REGb-=*#REGhl (Y  speed).
 $6C07 SUB (HL)      ;
 $6C08 LD B,A        ; }
 $6C09 LD A,C        ; {#REGc-=*#REGhl (X  speed).
 $6C0A SUB (HL)      ;
 $6C0B LD C,A        ; }
; Continue on to #R$6C0C.

; Validate Red Bird Position
;
; Used by the routines at #R$69F7, #R$6AF6, #R$6BC8 and #R$74A7.
; .
; Validates the red bird's proposed position (#REGb=Y, #REGc=X) against
; the room's flight path boundary data. Scans through the boundary table at
; *#R$6CBB looking for a region that contains the position.
; .
; Each entry is #N$04 bytes: Y-min, Y-max, X-min, X-max. If a valid region is
; found, the position is stored and carry is clear. If no valid region is found
; (entry is #N$00), carry is set.
;
;   B Proposed Y position
;   C Proposed X position
;   IX Pointer to red bird sprite data
; O:F Carry clear = valid, carry set = out of bounds
@label=ValidateRedBirdPosition
c$6C0C LD HL,($6CBB) ; #REGhl=*#R$6CBB (flight path boundary pointer).
@label=ValidateRedBirdPosition_Loop
*$6C0F PUSH HL       ; Stash the boundary pointer on the stack.
 $6C10 LD A,(HL)     ; #REGa=*#REGhl (Y-min for this region).
 $6C11 OR A          ; {Jump to #R$6C36 if #REGa is #N$00 (end of boundary
 $6C12 JR Z,$6C36    ; data).}
; Check Y-min <= B.
 $6C14 CP B          ; {Jump to #R$6C2F if #REGb is less than Y-min.
 $6C15 JR NC,$6C2F   ; }
; Check B <= Y-max.
 $6C17 INC HL        ; Advance to Y-max.
 $6C18 LD A,(HL)     ; #REGa=*#REGhl.
 $6C19 CP B          ; {Jump to #R$6C2F if #REGb is greater than Y-max.
 $6C1A JR C,$6C2F    ; }
; Check X-min <= C.
 $6C1C INC HL        ; Advance to X-min.
 $6C1D LD A,(HL)     ; #REGa=*#REGhl.
 $6C1E CP C          ; {Jump to #R$6C2F if #REGc is less than X-min.
 $6C1F JR NC,$6C2F   ; }
; Check C <= X-max.
 $6C21 INC HL        ; Advance to X-max.
 $6C22 LD A,(HL)     ; #REGa=*#REGhl.
 $6C23 CP C          ; {Jump to #R$6C2F if #REGc is greater than X-max.
 $6C24 JR C,$6C2F    ; }
; Position is within this region so store it and return success.
 $6C26 LD (IX+$00),C ; Write #REGc to *#REGix+#N$00 (X position).
 $6C29 LD (IX+$01),B ; Write #REGb to *#REGix+#N$01 (Y position).
 $6C2C POP HL        ; Restore the boundary pointer from the stack.
 $6C2D AND A         ; Clear carry (valid position).
 $6C2E RET           ; Return.
; Position is outside this region so try the next one.
@label=ValidateRedBirdPosition_Next
*$6C2F POP HL        ; Restore the boundary pointer from the stack.
 $6C30 INC HL        ; {Advance #REGhl by #N$04 (next boundary entry).
 $6C31 INC HL        ;
 $6C32 INC HL        ;
 $6C33 INC HL        ; }
 $6C34 JR $6C0F      ; Jump to #R$6C0F.
; End of boundary data. No valid region found.
@label=ValidateRedBirdPosition_OutOfBounds
*$6C36 POP HL        ; Restore the boundary pointer from the stack.
 $6C37 SCF           ; Set carry (out of bounds).
 $6C38 RET           ; Return.

; Generate Random Number
;
; Used by the routines at #R$69F7, #R$6AF6, #R$6E2F, #R$6F13, #R$6F54, #R$6F8D
; and #R$7028.
; .
; Generates a pseudo-random number using the seed stored at *#R$6CB7.
; The algorithm scrambles the seed value through a series of arithmetic
; operations and stores the result back as the new seed. Returns the random
; value in #REGa.
;
; O:A Pseudo-random value
@label=GenerateRandomNumber
c$6C39 LD A,($6CB7)  ; {#REGh=*#R$6CB7.
 $6C3C LD H,A        ; }
 $6C3D LD L,$00      ; {Load both #REGl and #REGd with #N$00.
 $6C3F LD D,L        ; }
 $6C40 LD E,H        ; Copy *#R$6CB7 into #REGe.
; #REGhl = seed  #N$100, #REGde = seed. Subtract #REGde twice.
 $6C41 AND A         ; Clear carry.
 $6C42 SBC HL,DE     ; #REGhl-=#REGde.
 $6C44 SBC HL,DE     ; #REGhl-=#REGde.
 $6C46 LD DE,$00FE   ; {#REGhl+=#N($00FE,$04,$04).
 $6C49 ADD HL,DE     ; }
; Combine the high and low bytes.
 $6C4A LD A,L        ; #REGa=#REGl.
 $6C4B SUB H         ; #REGa-=#REGh.
 $6C4C JR C,$6C4F    ; Jump to #R$6C4F if carry set.
 $6C4E DEC A         ; Decrement #REGa.
@label=GenerateRandomNumber_Store
*$6C4F LD ($6CB7),A  ; Write the new seed to *#R$6CB7.
 $6C52 RET           ; Return.

; Check Sprite Collision
;
; Used by the routines at #R$69F7, #R$6E5D, #R$7082, #R$74D6, #R$7512, #R$768C
; and #R$794A.
; .
; Checks if two sprites overlap by comparing their X and Y positions.
; Each sprite is assumed to be #N$10 pixels wide and #N$0C pixels tall.
;
;   IX Pointer to first sprite data
;   IY Pointer to second sprite data
; O:F Carry set = no collision, carry clear = collision
;
; First check horizontal overlap. The two sprites overlap in X if
; neither sprite's left edge is beyond the other's right edge.
@label=CheckSpriteCollision
c$6C53 LD A,(IX+$00) ; Load #REGa with the first sprite's X position (from
                     ; *#REGix+#N$00).
 $6C56 CP (IY+$00)   ; {Jump to #R$6C63 if the first sprite is to the right of the second
 $6C59 JR NC,$6C63   ; sprite (checking against *#REGiy+#N$00/ the second sprites X position).
                     ; }
; First sprite is to the left so check if its right edge reaches the
; second sprite.
 $6C5B ADD A,$10     ; Add #N$10 to the first sprite's X position (add assumed sprite
                     ; width).
 $6C5D CP (IY+$00)   ; {Jump to #R$6C6E if the two sprites overlap
 $6C60 JR NC,$6C6E   ; horizontally.}
 $6C62 RET           ; Return (there's no horizontal overlap; carry is set).
; First sprite is to the right so check if the second sprite's right
; edge reaches the first sprite.
@label=CheckSpriteCollision_TestRight
*$6C63 LD A,(IY+$00) ; {Load #REGa with the second sprite's X position (from *#REGiy+#N$00)
 $6C66 ADD A,$10     ; + #N$10 (adding the sprite width).}
 $6C68 CP (IX+$00)   ; {Jump to #R$6C6E if the two sprites overlap
 $6C6B JR NC,$6C6E   ; horizontally.}
 $6C6D RET           ; Return (there's no horizontal overlap; carry is set).
; The two sprites overlap horizontally; now check if there's any vertical
; overlap.
@label=CheckSpriteCollision_TestVertical
*$6C6E LD A,(IX+$01) ; Load #REGa with the first sprite's Y position
                     ; (*#REGix+#N$01).
 $6C71 CP (IY+$01)   ; {Jump to #R$6C7C if the first sprite is below the second sprite
 $6C74 JR NC,$6C7C   ; (checking against *#REGiy+#N$01/ the second sprites Y position).
                     ; }
; First sprite is above so check if its bottom edge reaches the second
; sprite.
 $6C76 ADD A,$0C     ; Add #N$0C to the first sprite's Y position (add assumed sprite
                     ; height).
 $6C78 CP (IY+$01)   ; {Return (with; carry clear meaning a collision, and carry set
 $6C7B RET           ; meaning no collision).}
; First sprite is below so check if the second sprite's bottom edge
; reaches the first sprite.
@label=CheckSpriteCollision_TestBelow
*$6C7C LD A,(IY+$01) ; {Load #REGa with the second sprite's Y position (from *#REGiy+#N$01)
 $6C7F ADD A,$0C     ; + #N$0C (adding the sprite height).}
 $6C81 CP (IX+$01)   ; {Return (with; carry clear meaning a collision, and carry set
 $6C84 RET           ; meaning no collision).}

; Check Egg Collision
;
; Used by the routines at #R$69F7, #R$6F54, #R$7190, #R$74D6, #R$7512 and
; #R$768C.
; .
; Checks if an egg overlaps with a sprite by comparing their positions.
; The egg is assumed to be #N$10 pixels wide and #N$10 pixels tall, while
; the target point is offset by #N$04 pixels in X and #N$06 pixels in Y
; to check against the centre of the target sprite.
;
;   IX Pointer to sprite data
;   IY Pointer to egg sprite data
; O:F Carry set = no collision, carry clear = collision
;
; First check horizontal overlap. The egg's centre point (X + #N$04)
; is compared against the sprite's horizontal range.
@label=CheckEggCollision
c$6C85 LD A,(IY+$00) ; {Load #REGb with the egg's X position (from *#REGiy+#N$00) + #N$04
 $6C88 ADD A,$04     ; (offset to the egg's centre).
 $6C8A LD B,A        ; }
 $6C8B LD A,(IX+$00) ; Load #REGa with the sprite's X position (from
                     ; *#REGix+#N$00).
 $6C8E CP B          ; Compare with the egg's centre X position.
 $6C8F CCF           ; Complement the carry flag.
 $6C90 RET C         ; Return if the sprite is to the right of the egg (no overlap; carry
                     ; is set).
; Sprite is to the left so check if its right edge reaches the egg's
; centre.
 $6C91 ADD A,$10     ; Add #N$10 to the sprite's X position (add sprite width).
 $6C93 CP B          ; Compare this with the egg's centre X position.
 $6C94 RET C         ; Return (with; carry clear meaning a collision, and carry set meaning
                     ; no collision).
; The two overlap horizontally; now check vertical overlap. The egg's
; centre point (Y + #N$06) is compared against the sprite's vertical
; range.
 $6C95 LD A,(IY+$01) ; {Load #REGb with the egg's Y position (from *#REGiy+#N$01) + #N$06
 $6C98 ADD A,$06     ; (offset to the egg's centre).
 $6C9A LD B,A        ; }
 $6C9B LD A,(IX+$01) ; Load #REGa with the sprite's Y position (from
                     ; *#REGix+#N$01).
 $6C9E CP B          ; Compare this with the egg's centre Y position.
 $6C9F CCF           ; Complement the carry flag.
 $6CA0 RET C         ; Return if the sprite is below the egg (no overlap; carry
                     ; is set).
; Sprite is above so check if its bottom edge reaches the egg's centre.
 $6CA1 ADD A,$10     ; Add #N$10 to the sprite's Y position (add sprite
                     ; height).
 $6CA3 CP B          ; Compare this with the egg's centre Y position.
 $6CA4 RET           ; Return (with; carry clear meaning a collision, and carry set meaning
                     ; no collision).

; Reset Red Bird
;
; Used by the routine at #R$69A7.
; .
; Resets the red bird to an inactive state by writing #N$FF to its
; traversal direction, stun timers and current room. This effectively disables
; the red bird until it is reinitialised.
@label=ResetRedBird
c$6CA5 LD HL,$6CB3   ; {Write #N$FF to *#R$6CB3.
 $6CA8 LD C,$FF      ;
 $6CAA LD (HL),C     ; }
 $6CAB INC HL        ; {Write #N$FF to *#R$6CB4.
 $6CAC LD (HL),C     ; }
 $6CAD INC HL        ; {Write #N$FF to *#R$6CB5.
 $6CAE LD (HL),C     ; }
 $6CAF INC HL        ; {Write #N$FF to *#R$6CB6.
 $6CB0 LD (HL),C     ; }
 $6CB1 RET           ; Return.

