; from the stack and update the sprite frame.
@label=MainGameLoop_NoCollision
 $61E4 POP BC          ; Restore Percy's previous position from the stack.
 $61E5 LD (IX+$02),$07 ; Write #N$07 to *#REGix+#N$02.
; If Percy is not falling, check for ground beneath.
 $61E9 LD A,($5FA7)    ; {Jump to #R$623D if Percy is falling (*#R$5FA7 is
 $61EC OR A            ; set).
 $61ED JR NZ,$623D     ; }
; Check if there is ground below Percy's feet. Calculate the attribute
; address one character row below Percy's position.
@label=MainGameLoop_CheckGround
 $61EF LD C,(IX+$00)   ; #REGc=*#REGix+#N$00 (Percy's X position).
 $61F2 LD B,(IX+$01)   ; #REGb=*#REGix+#N$01 (Percy's Y position)..
 $61F5 LD A,C          ; #REGa=#REGc.
 $61F6 AND %00000111   ; Keep only bits 0-2 (sub-character X offset).
 $61F8 PUSH AF         ; Stash the X offset on the stack.
 $61F9 LD A,C          ; #REGa=#REGc.
 $61FA RRCA            ; {Rotate right three positions (divide X by #N$08).
 $61FB RRCA            ;
 $61FC RRCA            ; }
 $61FD AND %00011111   ; Keep only bits 0-4 (character column).
 $61FF LD L,A          ; #REGl=#REGa.
; Add #N$10 pixels to the Y position to check below Percy's feet.
 $6200 LD A,B          ; #REGa=#REGb.
 $6201 ADD A,$10       ; {#REGb=#REGa + #N$10.
 $6203 LD B,A          ; }
 $6204 RLCA            ; {Rotate left two positions.
 $6205 RLCA            ; }
 $6206 AND %11100000   ; Keep only bits 5-7.
 $6208 OR L            ; Merge in the column.
 $6209 LD L,A          ; #REGl=#REGa.
 $620A LD A,B          ; #REGa=#REGb.
 $620B AND %11000000   ; Keep only bits 6-7.
 $620D RRCA            ; {Rotate right three positions.
 $620E RRCA            ;
 $620F RRCA            ; }
 $6210 OR %11000000    ; Set bits 6-7 for the room attribute buffer at #R$C000.
 $6212 LD H,A          ; #REGh=#REGa.
 $6213 POP AF          ; Restore the X offset from the stack.
