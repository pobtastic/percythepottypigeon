 $6AA4 LD B,A          ; }
 $6AA5 LD A,($6CB6)    ; #REGa=*#R$6CB6.
; Check if the combined value wraps past room #N$0B.
 $6AA8 ADD A,B         ; #REGa+=#REGb.
 $6AA9 CP $0C          ; {Jump to #R$6AB5 if the combined value doesn't wrap
 $6AAB JR NZ,$6AB5     ; past room #N$0B.}
