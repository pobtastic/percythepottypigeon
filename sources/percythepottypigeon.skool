; sound.
 $6A3E CALL $679C      ; Call #R$679C.
 $6A41 LD DE,$0003     ; #REGde=#N($0003,$04,$04).
 $6A44 LD HL,$00C8     ; #REGhl=#N($00C8,$04,$04) (initial beep pitch).
 $6A47 LD B,$0A        ; Set a sound step counter in #REGb of #N$0A steps.
@label=HandleRedBird_StolenSound
