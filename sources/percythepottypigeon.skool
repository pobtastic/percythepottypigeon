 $62A6 LD A,($5FAA)  ; {Jump to #R$62B6 if *#R$5FAA is unset.
 $62A9 OR A          ;
 $62AA JR Z,$62B6    ; }
 $62AC LD A,(IX+$00) ; {Load #REGa with Percy's X position (*#REGix+#N$00) + the number of
 $62AF ADD A,D       ; pixels to move (from #REGd) + #N$04.
 $62B0 ADD A,$04     ; }
; If Percy has gone past the right edge transition into the next room.
 $62B2 CP $EF        ; {Jump to #R$6307 if Percy's new position is greater than or equal to
 $62B4 JR NC,$6307   ; #N$EF.}
@label=MovePercyRight_Apply
*$62B6 LD A,(IX+$00) ; {Load #REGa with Percy's X position (*#REGix+#N$00) + the number of
 $62B9 ADD A,D       ; pixels to move (from #REGd).}
; If Percy has gone past the right edge transition into the next room.
 $62BA CP $EF        ; {Jump to #R$6307 if Percy's new position is greater than or equal to
 $62BC JR NC,$6307   ; #N$EF (transition to next room).}
 $62BE LD (IX+$00),A ; Write Percy's updated X screen position back to
                     ; *#REGix+#N$00.
 $62C1 RET           ; Return.

; Move Percy Up
;
; Used by the routine at #R$5FE9.
; .
; Move Percy up. Clamps to Y position #N$00 at the top of the screen.
;
; IX Pointer to Percys state data
; D Number of pixels to move
@label=MovePercyUp
c$62C2 LD A,(IX+$01) ; {Load #REGa with Percy's Y position (*#REGix+#N$01) - the number of
 $62C5 SUB D         ; pixels to move (from #REGd).}
 $62C6 CP $A0        ; {Jump to #R$62CC if Percy's new position is less than #N$A0 (is
 $62C8 JR C,$62CC    ; Percy within the screen boundaries).}
; Percy is outside of the screen boundaries!
 $62CA LD A,$00      ; Clamp Percy to Y position #N$00 (the top of screen).
@label=MovePercyUp_Store
*$62CC LD (IX+$01),A ; Write Percy's updated Y screen position back to
                     ; *#REGix+#N$01.
 $62CF RET           ; Return.

; Move Percy Down
;
; Used by the routine at #R$5FE9.
;
; IX Pointer to Percys state data
; D Number of pixels to move
@label=MovePercyDown
c$62D0 LD A,(IX+$01) ; Load #REGa with Percy's Y position (*#REGix+#N$01).
 $62D3 ADD A,D       ; {Write #REGa + the number of pixels to move (from #REGd) back to
 $62D4 LD (IX+$01),A ; *#REGix+#N$01.}
 $62D7 RET           ; Return.

; Update Movement Speed
;
; Used by the routine at #R$5FE9.
; .
; Update Percy's movement speed.
; .
; If the current input matches the previous input, accelerate up to a maximum
; of #N$04. If the input has changed, decelerate down to a minimum of #N$01.
@label=UpdateMovementSpeed
c$62D8 LD A,($5FA2)  ; {Jump to #R$62ED if *#R$5FA4 is the same as *#R$5FA2.
 $62DB LD C,A        ;
 $62DC LD A,($5FA4)  ;
 $62DF CP C          ;
 $62E0 JR Z,$62ED    ; }
; Input changed so decelerate.
 $62E2 LD A,($5FAE)  ; #REGa=*#R$5FAE.
 $62E5 CP $01        ; {Return if already at minimum speed (#N$01).
 $62E7 RET Z         ; }
 $62E8 DEC A         ; Decrease speed by one.
 $62E9 LD ($5FAE),A  ; Write #REGa to *#R$5FAE.
 $62EC RET           ; Return.
; Input is the same so accelerate.
@label=UpdateMovementSpeed_Accelerate
*$62ED LD A,($5FAE)  ; #REGa=*#R$5FAE.
 $62F0 CP $04        ; {Return if already at maximum speed (#N$04).
 $62F2 RET Z         ; }
 $62F3 INC A         ; Increase speed by one.
 $62F4 LD ($5FAE),A  ; Write #REGa to *#R$5FAE.
 $62F7 RET           ; Return.
; This entry point is used by the routine at #R$5FE9.
; .
; No input so decelerate movement speed towards minimum.
@label=UpdateMovementSpeed_Decelerate
*$62F8 LD A,($5FAE)  ; #REGa=*#R$5FAE.
 $62FB CP $01        ; {Return if already at minimum speed (#N$01).
 $62FD RET Z         ; }
 $62FE DEC A         ; Decrease speed by one.
 $62FF LD ($5FAE),A  ; Write #REGa to *#R$5FAE.
 $6302 LD A,($5FA4)  ; {#REGe=*#R$5FA4 (use the previous direction for
 $6305 LD E,A        ; animation).}
 $6306 RET           ; Return.

; Transition Room Right
;
; Used by the routine at #R$62A2.
; .
; Percy has gone past the right edge so transition into the next room.
@label=TransitionRoomRight
c$6307 PUSH AF         ; Stash #REGaf on the stack.
 $6308 LD A,($5FC5)    ; #REGa=*#R$5FC5.
; If the current room is #N$0B (the last room), wrap to room #N$01.
 $630B CP $0B          ; {Jump to #R$6310 if this isn't room #N$0B.
 $630D JR NZ,$6310     ; }
 $630F XOR A           ; #REGa=#N$00 (will become #N$01 after increment).
@label=TransitionRoomRight_SetRoom
*$6310 LD (IX+$00),$00 ; Write #N$00 to *#REGix+#N$00 (reset Percy's X to left
                       ; edge).
 $6314 INC A           ; Increment room number.
 $6315 LD ($5FC5),A    ; Write #REGa to *#R$5FC5.
 $6318 CALL $5E24      ; Call #R$5E24 to draw the new room.
 $631B POP AF          ; Restore #REGaf from the stack.
 $631C POP AF          ; {Discard two stack values.
 $631D POP AF          ; }
 $631E RET             ; Return.

; Transition Room Left
;
; Used by the routine at #R$6280.
; .
; Percy has gone past the left edge so transition into the previous room.
@label=TransitionRoomLeft
c$631F PUSH AF         ; Stash #REGaf on the stack.
 $6320 LD A,($5FC5)    ; #REGa=*#R$5FC5.
; If the current room is room #N$01 wrap around to room #N$0B.
 $6323 CP $01          ; {Jump to #R$6329 if this isn't room #N$01.
 $6325 JR NZ,$6329     ; }
 $6327 LD A,$0C        ; Set #REGa to room #N$0C (will become #N$0B after
                       ; decrement).
@label=TransitionRoomLeft_SetRoom
*$6329 LD (IX+$00),$EE ; Write #N$EE to *#REGix+#N$00 (reset Percy's X to right
                       ; edge).
 $632D DEC A           ; Decrement room number.
 $632E LD ($5FC5),A    ; Write #REGa to *#R$5FC5.
 $6331 POP AF          ; Restore #REGaf from the stack.
 $6332 CALL $5E24      ; Call #R$5E24 to draw the new room.
 $6335 POP AF          ; {Discard two stack values.
 $6336 POP AF          ; }
 $6337 RET             ; Return.

; Update Percy Animation
;
; Used by the routine at #R$5FE9.
; .
; Update Percy's animation frame based on the current movement state and
; direction. The animation counter at *#R$5FB0 cycles through frames, with bit
; 7 indicating the second half of the cycle.
@label=UpdatePercyAnimation
@label=UpdatePercyAnimation
c$6338 LD A,($5FB0)  ; #REGa=*#R$5FB0.
 $633B BIT 7,A       ; {Jump to #R$6349 if in the second half of the cycle.
 $633D JR NZ,$6349   ; }
; First half so increment the counter.
 $633F INC A         ; Increment #REGa.
 $6340 CP $05        ; {Jump to #R$6356 if the counter has reached #N$05 (switch to second
 $6342 JR Z,$6356    ; half).}
 $6344 LD ($5FB0),A  ; Write #REGa to *#R$5FB0.
 $6347 JR $6362      ; Jump to #R$6362.
; Second half so decrement the counter.
@label=UpdatePercyAnimation_SecondHalf
*$6349 AND %00000111 ; Keep only bits 0-2 (frame index).
 $634B DEC A         ; Decrement the frame index.
 $634C OR A          ; {Jump to #R$635D if the frame index has reached zero.
 $634D JR Z,$635D    ; }
 $634F ADD A,$80     ; {Write #REGa + #N$80 (keep bit 7 set) to *#R$5FB0.
 $6351 LD ($5FB0),A  ; }
 $6354 JR $6362      ; Jump to #R$6362.
; Switch to the second half of the animation cycle.
@label=UpdatePercyAnimation_SwitchToSecondHalf
*$6356 LD A,$84      ; {Write #N$84 to *#R$5FB0.
 $6358 LD ($5FB0),A  ; }
 $635B JR $6362      ; Jump to #R$6362.
; Switch back to the first half of the animation cycle.
@label=UpdatePercyAnimation_SwitchToFirstHalf
*$635D LD A,$01      ; {Write #N$01 to *#R$5FB0.
 $635F LD ($5FB0),A  ; }
; Select Percy's sprite frame based on direction and animation state.
; Frames are arranged in groups: #TABLE(default,centre,centre)
; { =h Byte Range | =h State }
; { #N$01-#N$04 | Flying right }
; { #N$05-#N$08 | Flying left }
; { #N$09-#N$0C | Walking/ Standing right }
; { #N$0D-#N$10 | Walking/ Standing left }
; TABLE#
@label=UpdatePercyAnimation_SetFrame
*$6362 LD A,(IX+$00) ; {Jump to #R$6377 if Percy's current X position (*#REGix+#N$00) is the
 $6365 CP (IX+$40)   ; same as his previous X position (*#REGix+#N$40).}
 $6368 JR Z,$6377    ;
; Percy has moved horizontally so cycle the wing flap counter between
; .
; #N$01 and #N$04.
 $636A LD A,($5FAF)  ; #REGa=*#R$5FAF.
 $636D INC A         ; Increment the flap counter.
 $636E CP $05        ; {Jump to #R$6374 if it hasn't reached #N$05 yet.
 $6370 JR NZ,$6374   ; }
 $6372 LD A,$01      ; Reset the flap counter to #N$01.
@label=UpdatePercyAnimation_StoreFlap
*$6374 LD ($5FAF),A  ; Write #REGa to *#R$5FAF.
; Determine whether Percy is airborne or grounded, and facing left or
; right.
@label=UpdatePercyAnimation_ChooseFrame
*$6377 LD A,(IX+$01) ; {Jump to #R$639E if Percy's Y position (*#REGix+#N$01) is on the
 $637A CP $90        ; ground, or if *#R$5FAD is set (and percy has landed on a platform).
 $637C JR NC,$639E   ;
 $637E LD A,($5FAD)  ;
 $6381 OR A          ;
 $6382 JR NZ,$639E   ; }
; Percy is airborne so select flying frame based on direction.
 $6384 LD A,($5FA5)  ; {Jump to #R$6393 if *#R$5FA5 is unset (Percy is facing
 $6387 OR A          ; right).
 $6388 JR Z,$6393    ; }
; Flying left: frame = flap counter + #N$00.
 $638A LD A,($5FB0)  ; #REGa=*#R$5FB0.
 $638D AND %00000111 ; Keep only bits 0-2 (animation frame index).
 $638F LD (IX+$03),A ; Write #REGa to *#REGix+#N$03.
 $6392 RET           ; Return.
; Flying right: frame = flap counter + #N$04.
@label=UpdatePercyAnimation_FlyingRight
*$6393 LD A,($5FB0)  ; #REGa=*#R$5FB0.
 $6396 AND %00000111 ; Keep only bits 0-2.
 $6398 ADD A,$04     ; {Write #REGa + #N$04 to *#REGix+#N$03.
 $639A LD (IX+$03),A ; }
 $639D RET           ; Return.
; Percy is on the ground or landed so select grounded frame.
@label=UpdatePercyAnimation_Grounded
*$639E LD A,$01      ; {Write #N$01 to *#R$5FAE (reset movement speed).
 $63A0 LD ($5FAE),A  ; }
 $63A3 LD A,($5FA5)  ; {Jump to #R$63B2 if *#R$5FA5 is unset (Percy is facing
 $63A6 OR A          ; right).
 $63A7 JR Z,$63B2    ; }
; Grounded left: frame = flap counter + #N$08.
 $63A9 LD A,($5FAF)  ; {Write *#R$5FAF + #N$08 to *#REGix+#N$03.
 $63AC ADD A,$08     ;
 $63AE LD (IX+$03),A ; }
 $63B1 RET           ; Return.
; Grounded right: frame = flap counter + #N$0C.
@label=UpdatePercyAnimation_GroundedRight
*$63B2 LD A,($5FAF)  ; {Write *#R$5FAF + #N$0C to *#REGix+#N$03.
 $63B5 ADD A,$0C     ;
 $63B7 LD (IX+$03),A ; }
 $63BA RET           ; Return.

; Print HUD Header
;
; Used by the routine at #R$6438.
; .
; Set header-drawn flag, open channel #N$02, print Energy/Lives/Score
; string at #R$63E9. Then #R$63CB colours bottom rows.
@label=Print_HUD_Header
c$63BB LD A,$01      ; {Write #N$01 to *#R$5FA1.
 $63BD LD ($5FA1),A  ; }
; Open channel #N$02 (upper screen).
 $63C0 CALL $1601    ; #HTML(Call <a rel="noopener nofollow"
                     ; href="https://skoolkit.ca/disassemblies/rom/hex/asm/1601.html">CHAN_OPEN</a>.)
 $63C3 LD HL,$63E9   ; #REGhl=#R$63E9.
 $63C6 LD B,$4F      ; Set a counter in #REGb for the length of the string to print (#N$4F
                     ; characters).
 $63C8 CALL $6621    ; Call #R$6621.
; This entry point is used by the routine at #R$6438.
; .
; Use the Memory Refresh Register as a pointer offset into memory to
; source pseudo-random values.
*$63CB LD A,R        ; {#REGe=the contents of the Memory Refresh Register.
 $63CD LD E,A        ; }
 $63CE LD D,$00      ; #REGd=#N$00.
 $63D0 LD B,$20      ; Set a counter in #REGb for #N$20 attribute cells.
; Point to the bottom two rows of the attribute buffer.
 $63D2 LD HL,$5AC0   ; #REGhl=#N$5AC0 (attribute buffer location).
@label=Colour_Loop
*$63D5 LD A,(DE)     ; #REGa=*#REGde.
 $63D6 AND %00000111 ; Keep only bits 0-2.
 $63D8 INC DE        ; Increment #REGde by one.
 $63D9 CP $03        ; {Jump to #R$63D5 if #REGa is less than #N$03.
 $63DB JR C,$63D5    ; }
 $63DD PUSH DE       ; Stash #REGde on the stack.
 $63DE LD E,A        ; #REGe=#REGa.
 $63DF LD A,(HL)     ; #REGa=*#REGhl.
 $63E0 AND %11110111 ; Keep only bits 0-2, 4-7.
 $63E2 OR E          ; Set the bits from #REGe.
 $63E3 LD (HL),E     ; Write #REGe to *#REGhl.
 $63E4 POP DE        ; Restore #REGde from the stack.
 $63E5 INC HL        ; Increment #REGhl by one.
 $63E6 DJNZ $63D5    ; Decrease counter by one and loop back to #R$63D5 until
                     ; counter is zero.
 $63E8 RET           ; Return.

