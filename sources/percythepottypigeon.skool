; Pause Border Effect
;
; Used by the routine at #R$5DC0.
; .
; Displays a rainbow-like cycling colour border while waiting for the
; player to press "S" to unpause the game.
@label=Pause_BorderEffect
c$6966 EI            ; Enable interrupts.
 $6967 LD B,$14      ; {Pause for #N$14 frames to give the player a moment before the
@label=Pause_BorderEffect_ShortLoop
*$6969 HALT          ; effect begins.
 $696A DJNZ $6969    ; }
@label=Pause_BorderEffect_ColourCycle
*$696C LD B,$0E      ; Set a frame counter in #REGb of #N$0E frames.
 $696E HALT          ; Halt operation (suspend CPU until the next interrupt).
; Derive a border colour (#N$00-#N$07) from the current frame loop
; counter.
@label=Pause_BorderEffect_ColourLoop
*$696F LD A,B        ; {Load #REGa with bits 0-2 of the current frame counter value. This
 $6970 AND %00000111 ; limits the value to be between #N$00-#N$07 (i.e. the colour palette).
                     ; }
 $6972 PUSH BC       ; Stash the frame counter on the stack.
@label=Pause_Border_PauseLoop_01
*$6973 DJNZ $6973    ; Decrease the frame counter by one and loop until the counter is
                     ; zero.
 $6975 POP BC        ; Restore the frame counter from the stack.
 $6976 PUSH BC       ; But keep a copy of it back on the stack.
 $6977 LD B,$00      ; Set #REGb to #N$00 which increases the size of the
                     ; counter.
@label=Pause_Border_PauseLoop_02
*$6979 DJNZ $6979    ; Decrease the frame counter by one and loop again until the counter
                     ; is zero.
 $697B POP BC        ; Restore the frame counter from the stack.
 $697C OUT ($FE),A   ; Set border to the colour held by #REGa.
 $697E DJNZ $696F    ; Decrease counter by one and loop back to #R$696F until
                     ; counter is zero.
 $6980 LD A,$FD      ; {Read from the keyboard;
 $6982 IN A,($FE)    ; #TABLE(default,centre,centre,centre,centre,centre,centre)
                     ; { =h,r2 Port Number | =h,c5 Bit }
                     ; { =h 0 | =h 1 | =h 2 | =h 3 | =h 4 }
                     ; { #N$FD | A | S | D | F | G }
                     ; TABLE#}
 $6984 AND %00000010 ; Keep only the bit which relates to the "S" key (bit 1).
 $6986 JR NZ,$696C   ; Jump back to #R$696C if the "S" key was not pressed.
 $6988 LD A,$FD      ; {Read from the keyboard again;
@label=StartKey_Debounce
*$698A IN A,($FE)    ; #TABLE(default,centre,centre,centre,centre,centre,centre)
                     ; { =h,r2 Port Number | =h,c5 Bit }
