 $6196 LD A,C          ; #REGa=#REGc.
 $6197 AND %00000111   ; Keep only bits 0-2 (sub-character X offset).
 $6199 OR A            ; {Jump to #R$619E if #REGa is zero (aligned to
 $619A JR Z,$619E      ; character).}
