@label=DrawNestChicks_Loop
*$65EF PUSH BC       ; Stash the chick counter on the stack.
 $65F0 PUSH HL       ; Stash the screen position on the stack.
 $65F1 LD HL,$A763   ; #REGhl=#R$A763 (base address of the chick animation
                     ; frames).
 $65F4 LD BC,$0008   ; #REGbc=#N$0008 (bytes per frame).
; Use the rotating bit pattern at #R$5FB4 to select which animation frame
; to use for this chick. The pattern is rotated each time, so each chick picks
; up a different bit and animates independently.
 $65F7 LD A,($5FB4)  ; {Update *#R$5FB4 by rotating the frame selection bits.
 $65FA RRCA          ;
 $65FB LD ($5FB4),A  ; }
 $65FE JR C,$6601    ; Jump to #R$6601 if carry is set (use first frame at
                     ; #R$A763).
 $6600 ADD HL,BC     ; #REGhl+=#REGbc (use second frame at #R$A76B).
@label=DrawNestChicks_DrawFrame
*$6601 EX DE,HL      ; Transfer the frame graphic address to #REGde.
 $6602 POP HL        ; Restore the screen position from the stack.
 $6603 PUSH HL       ; Keep a copy of the screen position on the stack.
 $6604 CALL $6692    ; Call #R$6692 to draw the chick frame to the screen.
 $6607 POP HL        ; {Restore the screen position and chick counter from the
 $6608 POP BC        ; stack.}
 $6609 INC L         ; Move one character column to the right for the next
                     ; chick.
 $660A DJNZ $65EF    ; Decrease the chick counter by one and loop back to #R$65EF until all
                     ; #N$03 chicks are drawn.
 $660C RET           ; Return.

