; Handler: Car Type 1 (Driving Right)
;
; Used by the routine at #R$6DAB.
; .
; Handles a car driving right across the screen. The car is made up of
; two sprites (#N$10 pixels apart) representing the front and back.
; .
; When the car reaches the right edge it is reinitialised from a random
; position.
@label=Handler_Car_Type1
c$6E5D LD IX,$DAC8     ; #REGix=#R$DAC8.
 $6E61 LD A,($5FBB)    ; {Call #R$6F13 if *#R$5FBB is set/ the room is being
 $6E64 OR A            ; initialised.
 $6E65 CALL NZ,$6F13   ; }
; If the car crash timer is active, handle the crash animation.
 $6E68 LD A,($7208)    ; #REGa=*#R$7208.
 $6E6B OR A            ; {Jump to #R$6E97 if the crash timer is zero (car is driving
 $6E6C JR Z,$6E97      ; normally).}
; This entry point is used by the routines at #R$6F8D and #R$7028.
*$6E6E CP $01          ; {Jump to #R$6E91 if the crash timer has reached #N$01 (crash
 $6E70 JR Z,$6E91      ; ending).}
; Car is crashing  decrement the timer and cycle through crash
; animation frames.
 $6E72 DEC A           ; Decrement the crash timer.
 $6E73 LD ($7208),A    ; Write #REGa to *#R$7208.
 $6E76 AND %00000010   ; Keep only bit 1 (alternates between #N$00 and #N$02).
 $6E78 ADD A,$20       ; #REGa+=#N$20 (crash frame base).
 $6E7A EX AF,AF'       ; Stash the frame in shadow #REGaf.
; If the car is on the lower road (Y=#N$A0), use a different frame set.
 $6E7B LD A,(IX+$01)   ; Load #REGa with the car's Y position (from
                       ; *#REGix+#N$01).
 $6E7E CP $A0          ; {Jump to #R$6E87 if the car is not on the lower road.
 $6E80 JR NZ,$6E87     ; }
 $6E82 EX AF,AF'       ; Retrieve the crash frame from shadow #REGaf.
 $6E83 ADD A,$02       ; #REGa+=#N$02 (offset to left-facing crash frames).
 $6E85 JR $6E88        ; Jump to #R$6E88.
@label=Handler_CarType1_SetCrashFrame
*$6E87 EX AF,AF'       ; Retrieve the crash frame from shadow #REGaf.
@label=Handler_CarType1_StoreCrashFrame
*$6E88 LD (IX+$03),A   ; Write #REGa to *#REGix+#N$03 (front sprite frame).
 $6E8B INC A           ; Increment #REGa for the rear sprite.
 $6E8C LD (IX+$07),A   ; Write #REGa to *#REGix+#N$07 (rear sprite frame).
 $6E8F JR $6EEF        ; Jump to #R$6EEF.
; Crash animation finished  clear the timer.
@label=Handler_CarType1_CrashEnd
*$6E91 XOR A           ; {Write #N$00 to *#R$7208.
 $6E92 LD ($7208),A    ; }
 $6E95 JR $6EEF        ; Jump to #R$6EEF.
; Car is driving normally  move it to the right.
@label=Handler_CarType1_Drive
*$6E97 LD HL,$720C     ; #REGhl=#R$720C (car speed).
 $6E9A LD A,(IX+$00)   ; #REGa=*#REGix+#N$00 (car X position).
 $6E9D ADD A,(HL)      ; #REGa+=*#REGhl (add speed).
 $6E9E CP $DE          ; {Call #R$6F13 to reinitialise the car if it's at the
 $6EA0 CALL NC,$6F13   ; right edge.}
; Update both sprite X positions (#N$10 apart).
 $6EA3 LD (IX+$00),A   ; Write #REGa to *#REGix+#N$00 (front X).
 $6EA6 ADD A,$10       ; #REGa+=#N$10.
 $6EA8 LD (IX+$04),A   ; Write #REGa to *#REGix+#N$04 (rear X).
; Set the driving right frames.
 $6EAB LD (IX+$03),$1C ; Write #N$1C to *#REGix+#N$03 (front frame: driving
                       ; right).
 $6EAF LD (IX+$07),$1D ; Write #N$1D to *#REGix+#N$07 (rear frame: driving
                       ; right).
; Set the car colour for both sprites.
 $6EB3 LD A,($720A)    ; {Write *#R$720A to; #LIST
 $6EB6 LD (IX+$02),A   ; { *#REGix+#N$02 (front colour) }
 $6EB9 LD (IX+$06),A   ; { *#REGix+#N$06 (rear colour) }
                       ; LIST#}
; This entry point is used by the routine at #R$6F8D.
; .
; Set the Y positions for the wheels/underside sprites.
*$6EBC LD (IX+$21),$A0 ; Write #N$A0 to *#REGix+#N$21 (front underside Y).
 $6EC0 LD (IX+$25),$A0 ; Write #N$A0 to *#REGix+#N$25 (rear underside Y).
; Set the X positions for the underside sprites (offset by #N$03 and
; #N$18 from the car's X).
 $6EC4 LD A,(IX+$00)   ; #REGa=*#REGix+#N$00.
 $6EC7 ADD A,$03       ; #REGa+=#N$03.
 $6EC9 LD (IX+$20),A   ; Write #REGa to *#REGix+#N$20 (front underside X).
 $6ECC ADD A,$15       ; #REGa+=#N$15.
 $6ECE LD (IX+$24),A   ; Write #REGa to *#REGix+#N$24 (rear underside X).
; Animate the wheels  cycle through frames #N$10-#N$13.
 $6ED1 LD HL,$720D     ; #REGhl=#R$720D (wheel animation counter).
 $6ED4 LD A,(HL)       ; #REGa=*#REGhl.
 $6ED5 INC A           ; Increment the counter.
 $6ED6 AND %00000011   ; Keep only bits 0-1 (cycle #N$00-#N$03).
 $6ED8 LD (HL),A       ; Write back to *#REGhl.
 $6ED9 ADD A,$10       ; #REGa+=#N$10 (wheel frame base).
 $6EDB LD (IX+$23),A   ; Write #REGa to *#REGix+#N$23 (front wheel frame).
; Offset the rear wheel animation by #N$02 for visual variety.
 $6EDE LD A,(HL)       ; #REGa=*#REGhl.
 $6EDF ADD A,$02       ; #REGa+=#N$02.
 $6EE1 AND %00000011   ; Keep only bits 0-1.
 $6EE3 ADD A,$10       ; #REGa+=#N$10 (wheel frame base).
 $6EE5 LD (IX+$27),A   ; Write #REGa to *#REGix+#N$27 (rear wheel frame).
; This entry point is used by the routine at #R$6F8D.
; .
; Check if Percy's egg has hit the car.
*$6EE8 LD A,($5FA9)    ; #REGa=*#R$5FA9.
 $6EEB OR A            ; {Call #R$6F54 if the egg is active.
 $6EEC CALL NZ,$6F54   ; }
; Check if the car has collided with Percy.
@label=Handler_CarCheckPercyCollision
*$6EEF LD IX,$DAC0     ; #REGix=#R$DAC0.
 $6EF3 LD IY,$DACC     ; #REGiy=#R$DACC.
; This entry point is used by the routine at #R$6DAB.
@label=Handler_CarCheckPercyCollision_Test
*$6EF7 CALL $6C53      ; Call #R$6C53.
 $6EFA RET C           ; Return if no collision.
; Car hit Percy  knock him backward.
@label=Handler_CarKnockBackPercy
 $6EFB LD HL,$DAC0     ; #REGhl=#R$DAC0 (Percy's X position).
 $6EFE LD A,(HL)       ; #REGa=*#REGhl.
 $6EFF SUB $14         ; #REGa-=#N$14 (knock back distance).
 $6F01 JR C,$6F0B      ; Jump to #R$6F0B if the result underflowed (Percy at
                       ; the left edge).
*$6F03 LD (HL),A       ; Write the new X position to *#REGhl.
 $6F04 INC HL          ; Advance to Percy's Y position.
 $6F05 LD (HL),$78     ; Write #N$78 to *#REGhl (set Percy's Y position after
                       ; knockback).
 $6F07 LD ($5FA7),A    ; Write #REGa to *#R$5FA7 (set the falling state).
 $6F0A RET             ; Return.
; Percy was too close to the left edge  adjust position rightward.
@label=Handler_CarKnockBackPercy_LeftEdge
*$6F0B LD A,(HL)       ; #REGa=*#REGhl.
 $6F0C ADD A,$0A       ; #REGa+=#N$0A.
 $6F0E JR $6F03        ; Jump to #R$6F03.
; Alternate entry point for car initialisation.
@label=InitialiseCar_Alternate
 $6F10 CALL $6F13      ; Call #R$6F13.

; Initialise Car
;
; Used by the routine at #R$6E5D.
; .
; Initialises a car with random speed, colour and position. The car
; starts at Y=#N$90 (upper road) with the possibility of appearing at different
; X positions.
;
; IX Pointer to car sprite data
@label=InitialiseCar
c$6F13 CALL $6C39      ; Call #R$6C39.
 $6F16 AND %00000011   ; Keep only bits 0-1 (random speed #N$00-#N$03).
 $6F18 INC A           ; Increment (speed #N$01-#N$04).
 $6F19 LD ($720C),A    ; Write #REGa to *#R$720C.
; Set a random appearance delay if the room is valid.
 $6F1C LD A,($5FC5)    ; #REGa=*#R$5FC5.
 $6F1F OR A            ; {Jump to #R$6F2C if the room number is zero.
 $6F20 JR Z,$6F2C      ; }
 $6F22 CALL $6C39      ; Call #R$6C39.
 $6F25 AND %00011111   ; Keep only bits 0-4.
 $6F27 OR %00000001    ; Set bit 0 (ensure odd value).
 $6F29 LD ($720E),A    ; Write #REGa to *#R$720E.
; Pick a random colour, but reject colour #N$01 (blue).
@label=InitialiseCar_PickColour
*$6F2C CALL $6C39      ; Call #R$6C39.
 $6F2F AND %00000011   ; Keep only bits 0-1 (random colour #N$00-#N$03).
 $6F31 CP $01          ; {Jump to #R$6F2C if the colour is #INK$01 (pick
 $6F33 JR Z,$6F2C      ; again).}
 $6F35 LD ($720A),A    ; Write #REGa to *#R$720A.
; Clear the crash timer and set the car on the upper road.
 $6F38 XOR A           ; {Write #N$00 to *#R$7208.
 $6F39 LD ($7208),A    ; }
 $6F3C LD (IX+$01),$90 ; Write #N$90 to *#REGix+#N$01 (front Y: upper road).
 $6F40 LD (IX+$05),$90 ; Write #N$90 to *#REGix+#N$05 (rear Y: upper road).
 $6F44 LD (IX+$22),$07 ; Write #N$07 to *#REGix+#N$22 (front wheel colour:
                       ; white).
 $6F48 LD (IX+$26),$07 ; Write #N$07 to *#REGix+#N$26 (rear wheel colour:
                       ; white).
; If the car's X position is past #N$64, reset it to the left.
 $6F4C LD A,(IX+$00)   ; Load #REGa with the car's X position (from
                       ; *#REGix+#N$00).
 $6F4F CP $64          ; {Return if the car's X position is less than #N$64 (position is
 $6F51 RET C           ; valid).}
 $6F52 XOR A           ; #REGa=#N$00 (reset X position to the left edge).
 $6F53 RET             ; Return.

; Check Car Egg Collision
;
; Used by the routine at #R$6E5D.
; .
; Checks if Percy's egg has hit the car. If so, starts the crash
; animation and awards points.
;
; IX Pointer to car sprite data
@label=CheckCarEggCollision
c$6F54 LD HL,$DAC8   ; #REGhl=#R$DAC8.
 $6F57 LD BC,$7208   ; #REGbc=#R$7208 (crash timer).
; This entry point is used by the routine at #R$6DAB.
@label=CheckCarEggCollision_Test
*$6F5A LD A,(HL)     ; #REGa=*#REGhl.
 $6F5B PUSH AF       ; Stash the original X position on the stack.
 $6F5C ADD A,$05     ; #REGa+=#N$05 (offset to centre of car).
 $6F5E LD (HL),A     ; Write the adjusted position to *#REGhl.
 $6F5F PUSH HL       ; {Copy #REGhl to #REGix.
 $6F60 POP IX        ; }
 $6F62 LD IY,$DAE0   ; #REGiy=#R$DAE0 (egg sprite data).
 $6F66 PUSH BC       ; Stash #REGbc on the stack.
 $6F67 CALL $6C85    ; Call #R$6C85.
 $6F6A POP BC        ; Restore #REGbc from the stack.
 $6F6B JR C,$6F8A    ; Jump to #R$6F8A if no collision.
; Egg hit the car  restore the original X position and start the crash.
 $6F6D POP AF        ; Restore the original X position from the stack.
 $6F6E LD (HL),A     ; Write it back to *#REGhl.
 $6F6F CALL $6C39    ; Call #R$6C39.
 $6F72 OR %00010000  ; Set bit 4 (ensure a minimum crash duration).
 $6F74 LD (BC),A     ; Write #REGa to *#REGbc (crash timer).
 $6F75 LD A,$0F      ; {Call #R$67B2 to add #N$0F points to the score.
 $6F77 CALL $67B2    ; }
; This entry point is used by the routines at #R$69F7, #R$7190, #R$74B9,
; #R$74D6 and #R$768C.
; .
; Play a short random sound burst  used as a hit sound effect by
; multiple routines.
@label=PlayHitSound
*$6F7A LD B,$00      ; #REGb=#N$00 (loop #N$100 times).
@label=PlayHitSound_Loop
*$6F7C CALL $6C39    ; Call #R$6C39.
 $6F7F AND %00011000 ; Keep only bits 3-4.
 $6F81 OR %00000001  ; Set bit 0 (border colour: blue).
 $6F83 OUT ($FE),A   ; Send to the speaker.
 $6F85 NOP           ; {No operation.
 $6F86 NOP           ; }
 $6F87 DJNZ $6F7C    ; Decrease #REGb by one and loop back to #R$6F7C until
                     ; done.
 $6F89 RET           ; Return.
; No collision  restore the original X position and return.
@label=CheckCarEggCollision_Miss
*$6F8A POP AF        ; Restore the original X position from the stack.
 $6F8B LD (HL),A     ; Write it back to *#REGhl.
 $6F8C RET           ; Return.

; Handler: Car Type 2 (Driving Left)
;
; Used by the routine at #R$6DAB.
; .
; Handles a car driving left across the screen. When it reaches the left
; boundary it turns around and drives right at ground level until reaching the
; right edge, then reinitialises.
@label=Handler_CarType2
c$6F8D LD IX,$DAC8     ; #REGix=#R$DAC8.
 $6F91 LD HL,$720B     ; #REGhl=#R$720B (car speed).
; If the room is being initialised, set up the car.
 $6F94 LD A,($5FBB)    ; #REGa=*#R$5FBB.
 $6F97 OR A            ; {Jump to #R$700C if the room buffer flag is set.
 $6F98 JR NZ,$700C     ; }
; If the car crash timer is active, handle the crash animation.
 $6F9A LD A,($7208)    ; #REGa=*#R$7208.
 $6F9D OR A            ; {Jump to #R$6E6E if the crash timer is active.
 $6F9E JP NZ,$6E6E     ; }
; Check which road the car is on.
 $6FA1 LD A,(IX+$01)   ; #REGa=*#REGix+#N$01 (car Y position).
 $6FA4 CP $90          ; Is the car on the upper road (#N$90)?
 $6FA6 JR Z,$6FCB      ; Jump to #R$6FCB if on the upper road.
; Car is on the lower road (Y=#N$A0)  driving left.
 $6FA8 LD A,(IX+$00)   ; #REGa=*#REGix+#N$00 (car X position).
 $6FAB SUB (HL)        ; #REGa-=*#REGhl (subtract speed).
 $6FAC CP $68          ; Has the car reached #N$68 (left boundary)?
 $6FAE JR C,$6FEE      ; Jump to #R$6FEE if at the left boundary (turn around).
; This entry point is used by the routine at #R$7028.
; .
; Update positions and set driving left frames.
@label=Handler_CarType2_UpdateLeft
*$6FB0 LD (IX+$00),A   ; Write #REGa to *#REGix+#N$00 (front X).
 $6FB3 ADD A,$10       ; #REGa+=#N$10.
 $6FB5 LD (IX+$04),A   ; Write #REGa to *#REGix+#N$04 (rear X).
 $6FB8 LD (IX+$01),$A0 ; Write #N$A0 to *#REGix+#N$01 (front Y: lower road).
 $6FBC LD (IX+$05),$A0 ; Write #N$A0 to *#REGix+#N$05 (rear Y: lower road).
 $6FC0 LD (IX+$03),$1E ; Write #N$1E to *#REGix+#N$03 (front frame: driving
                       ; left).
 $6FC4 LD (IX+$07),$1F ; Write #N$1F to *#REGix+#N$07 (rear frame: driving
                       ; left).
 $6FC8 JP $6EE8        ; Jump to #R$6EE8.
; Car is on the upper road (Y=#N$90)  driving right.
@label=Handler_CarType2_DriveRight
*$6FCB LD A,(IX+$00)   ; #REGa=*#REGix+#N$00 (car X position).
 $6FCE ADD A,(HL)      ; #REGa+=*#REGhl (add speed).
 $6FCF CP $DE          ; Has the car reached #N$DE (right edge)?
 $6FD1 JR NC,$700C     ; Jump to #R$700C if at the right edge (reinitialise).
; This entry point is used by the routine at #R$7028.
; .
; Update positions and set driving right frames.
@label=Handler_CarType2_UpdateRight
*$6FD3 LD (IX+$00),A   ; Write #REGa to *#REGix+#N$00 (front X).
 $6FD6 ADD A,$10       ; #REGa+=#N$10.
 $6FD8 LD (IX+$04),A   ; Write #REGa to *#REGix+#N$04 (rear X).
 $6FDB LD (IX+$01),$90 ; Write #N$90 to *#REGix+#N$01 (front Y: upper road).
 $6FDF LD (IX+$05),$90 ; Write #N$90 to *#REGix+#N$05 (rear Y: upper road).
 $6FE3 LD (IX+$03),$1C ; Write #N$1C to *#REGix+#N$03 (front frame: driving
                       ; right).
 $6FE7 LD (IX+$07),$1D ; Write #N$1D to *#REGix+#N$07 (rear frame: driving
                       ; right).
 $6FEB JP $6EBC        ; Jump to #R$6EBC.
; Car reached the left boundary  turn around. Pick a new random speed
; and colour, then start driving right from the left boundary.
@label=Handler_CarType2_TurnAround
*$6FEE PUSH HL         ; Stash #REGhl on the stack.
 $6FEF CALL $6C39      ; Call #R$6C39.
 $6FF2 POP HL          ; Restore #REGhl from the stack.
 $6FF3 AND %00000011   ; Keep only bits 0-1 (random speed #N$00-#N$03).
 $6FF5 INC A           ; Increment (speed #N$01-#N$04).
 $6FF6 LD (HL),A       ; Write the new speed to *#REGhl.
; Pick a new colour, rejecting #N$01 (blue).
*$6FF7 PUSH HL         ; Stash #REGhl on the stack.
 $6FF8 CALL $6C39      ; Call #R$6C39.
 $6FFB POP HL          ; Restore #REGhl from the stack.
 $6FFC AND %00000011   ; Keep only bits 0-1.
 $6FFE CP $01          ; Is the colour #N$01 (blue)?
 $7000 JR Z,$6FF7      ; Jump to #R$6FF7 if blue (pick again).
 $7002 LD (IX+$02),A   ; Write #REGa to *#REGix+#N$02 (front colour).
 $7005 LD (IX+$06),A   ; Write #REGa to *#REGix+#N$06 (rear colour).
 $7008 LD A,$68        ; #REGa=#N$68 (left boundary X position).
 $700A JR $6FD3        ; Jump to #R$6FD3.
; Initialise car type 2  start from the right edge driving left with
; a random speed and colour.
@label=Handler_CarType2_Initialise
*$700C PUSH HL         ; Stash #REGhl on the stack.
 $700D CALL $6C39      ; Call #R$6C39.
 $7010 POP HL          ; Restore #REGhl from the stack.
 $7011 AND %00000011   ; Keep only bits 0-1.
 $7013 INC A           ; Increment (speed #N$01-#N$04).
 $7014 LD (HL),A       ; Write the new speed to *#REGhl.
; Pick a colour, using the range #N$04-#N$07 (bright colours).
 $7015 PUSH HL         ; Stash #REGhl on the stack.
 $7016 CALL $6C39      ; Call #R$6C39.
 $7019 POP HL          ; Restore #REGhl from the stack.
 $701A AND %00000011   ; Keep only bits 0-1.
 $701C ADD A,$04       ; #REGa+=#N$04 (bright colour range).
 $701E LD (IX+$02),A   ; Write #REGa to *#REGix+#N$02 (front colour).
 $7021 LD (IX+$06),A   ; Write #REGa to *#REGix+#N$06 (rear colour).
 $7024 LD A,$DE        ; #REGa=#N$DE (right edge X position).
 $7026 JR $6FB0        ; Jump to #R$6FB0.

