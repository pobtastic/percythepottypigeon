 $6B36 CALL $6C39      ; Call #R$6C39.
 $6B39 PUSH AF         ; Stash #REGaf on the stack.
 $6B3A AND %00000111   ; Keep only bits 0-2 (random direction #N$00-#N$07).
 $6B3C LD ($6CB8),A    ; Write #REGa to *#R$6CB8.
 $6B3F POP AF          ; Restore #REGaf from the stack.
; Calculate the direction change timer from the random value.
 $6B40 RRCA            ; {Rotate right three positions.
 $6B41 RRCA            ;
 $6B42 RRCA            ; }
 $6B43 AND %01000000   ; Keep only bit 6.
 $6B45 OR %00000100    ; Set bit 2.
 $6B47 LD ($6CB2),A    ; Write #REGa to *#R$6CB2.
; Move the red bird in its current direction. The direction is doubled
; and self-modified into the jump table at #R$6BC8.
@label=UpdateRedBirdMovement_Move
*$6B4A LD A,($6CB8)    ; Load #REGa with *#R$6CB8.
 $6B4D ADD A,A         ; Double it (each jump table entry is #N$02 bytes).
 $6B4E LD ($6BC9),A    ; Write #REGa to *#R$6BC8(#N$6BC9) (self-modify the jump
                       ; offset).
 $6B51 LD C,(IX+$00)   ; #REGc=*#REGix+#N$00 (red bird's X position).
 $6B54 LD B,(IX+$01)   ; #REGb=*#REGix+#N$01 (red bird's Y position).
 $6B57 LD HL,$6CB9     ; #REGhl=#R$6CB9.
 $6B5A CALL $6BC8      ; Call #R$6BC8.
 $6B5D JR C,$6B2F      ; Jump to #R$6B2F if the carry flag is set (red bird left the valid
                       ; area).
; Update the red bird's wing animation frame.
@label=UpdateRedBirdMovement_Animate
*$6B5F LD HL,$6CC4     ; #REGhl=#R$6CC4.
 $6B62 LD A,(HL)       ; #REGa=*#REGhl.
 $6B63 INC A           ; Increment the animation counter.
 $6B64 AND %00000111   ; Keep only bits 0-2 (cycle through #N$00-#N$07).
 $6B66 LD (HL),A       ; Write back to *#REGhl.
 $6B67 RRA             ; Rotate right (divide by #N$02).
 $6B68 ADD A,$18       ; #REGa+=#N$18 (red bird animation frame base).
 $6B6A LD ($DAC7),A    ; Write #REGa to *#R$DAC7 (red bird frame).
 $6B6D RET             ; Return.
; Red bird is waiting to appear. Count down the delay timer and handle
; the room transition when it expires.
@label=UpdateRedBirdMovement_WaitToAppear
*$6B6E LD A,($5FC5)    ; {#REGb=*#R$5FC5.
 $6B71 LD B,A          ; }
 $6B72 LD A,($6CB6)    ; #REGa=*#R$6CB6 (red bird's current room).
 $6B75 CP B            ; Is the red bird in the same room as Percy?
 $6B76 JR NZ,$6B7E     ; Jump to #R$6B7E if not.
; Red bird has arrived in Percy's room so clear the delay and animate.
 $6B78 XOR A           ; {Write #N$00 to *#R$6CBD.
 $6B79 LD ($6CBD),A    ; }
 $6B7C JR $6B5F        ; Jump to #R$6B5F.
; Still waiting so decrement the appearance delay.
@label=UpdateRedBirdMovement_Countdown
