; Draw Room Rows
;
; Used by the routine at #R$5F35.
; .
; Loop B times: call #R$6456 (draw row), advance #REGhl. Then call #R$63BB
; if header not drawn, and #R$63CB to colour bottom rows.
;
; B Number of rows to draw
; HL Buffer to use
@label=DrawRows_Room
c$6438 PUSH BC       ; {Stash the row counter and room buffer on the stack.
 $6439 PUSH HL       ; }
 $643A CALL $6456    ; Call #R$6456.
 $643D POP HL        ; {Restore the room buffer and row countr from the stack.
 $643E POP BC        ; }
; Advance the room buffer pointer to the next character row. Each row is
; #N$20 bytes wide; if adding #N$20 to #REGl overflows, carry into #REGh by
; adding #N$08 (into the next screen third).
 $643F LD A,$20      ; {#REGl+=#N$20.
 $6441 ADD A,L       ;
 $6442 LD L,A        ; }
 $6443 OR A          ; {Jump to #R$644A if there was no overflow.
 $6444 JR NZ,$644A   ; }
 $6446 LD A,$08      ; {#REGh+=#N$08.
 $6448 ADD A,H       ;
 $6449 LD H,A        ; }
@label=DrawRoomRows_Next
*$644A DJNZ $6438    ; Decrease the row counter by one and loop back to #R$6438 until all
                     ; of the rows have been drawn to the screen buffer.
 $644C LD A,($5FA1)  ; {Call #R$63BB if *#R$5FA1 is zero.
 $644F OR A          ;
 $6450 CALL Z,$63BB  ; }
 $6453 JP $63CB      ; Jump to #R$63CB.

; Draw Room Row
;
; Used by the routine at #R$6438.
;
; HL Pointer to the room buffer row
@label=DrawRow_Room
c$6456 LD B,$08      ; Set a counter in #REGb for #N$08 pixel lines.
 $6458 PUSH HL       ; Stash the room buffer pointer on the stack.
@label=DrawRoomRow_PixelLoop
*$6459 PUSH BC       ; {Stash the line counter and room buffer pointer on the
 $645A PUSH HL       ; stack.}
 $645B LD D,H        ; {Copy the room buffer pointer to #REGde.
 $645C LD E,L        ; }
 $645D RES 7,D       ; Reset bit 7 of #REGd to convert from a room buffer address to a
                     ; screen buffer address.
 $645F LD BC,$0020   ; {Copy #N($0020,$04,$04) bytes of data from the room buffer to the
 $6462 LDIR          ; screen buffer.}
 $6464 POP HL        ; {Restore the room buffer pointer and line counter from
 $6465 POP BC        ; the stack.}
 $6466 INC H         ; Move down one pixel line in the room buffer.
 $6467 DJNZ $6459    ; Decrease the line counter by one and loop back to #R$6459 until all
                     ; #N$08 pixel lines are copied.
 $6469 POP HL        ; Restore the original room buffer pointer from the stack.
 $646A LD A,H        ; #REGa=#REGh.
 $646B RRCA          ; {Rotate right three positions to extract the character
 $646C RRCA          ; row.
 $646D RRCA          ; }
 $646E AND %00000011 ; Keep only bits 0-1.
 $6470 OR %11011000  ; Set bits 3-4, 6-7 to form the attribute buffer high
                     ; byte.
 $6472 LD H,A        ; #REGh=#REGa.
 $6473 LD E,L        ; #REGe=#REGl.
 $6474 LD A,H        ; #REGa=#REGh.
 $6475 AND %00000011 ; Keep only bits 0-1.
 $6477 OR %01011000  ; Set bits 3-4, 6 to form the destination attribute buffer high
                     ; byte.
 $6479 LD D,A        ; #REGd=#REGa.
 $647A LD BC,$0020   ; {Copy #N($0020,$04,$04) bytes to the attribute buffer.
 $647D LDIR          ; }
 $647F RET           ; Return.

