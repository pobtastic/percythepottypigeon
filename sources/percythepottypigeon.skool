g$5BFE DEFB $00

i$5BFF

; Game Initialise
@label=Game_Initialise
c$5DC0 DI              ; Disable interrupts.
 $5DC1 XOR A           ; Set #REGa to #N$00 for clearing the flags below.
; Put the stack pointer somewhere safe.
 $5DC2 LD SP,$5BFE     ; #REGsp=#R$5BFE.
 $5DC5 LD ($5FBC),A    ; {Write #N$00 to; #LIST
 $5DC8 LD ($5FAA),A    ; { *#R$5FBC }
                       ; { *#R$5FAA }
                       ; LIST#}
 $5DCB OUT ($FE),A     ; Set border to: #INK$00.
; #HTML(Open channel #N$02 (upper screen) via <a rel="noopener nofollow"
; href="https://skoolkit.ca/disassemblies/rom/hex/asm/1601.html">CHAN_OPEN</a>.)
 $5DCD CALL $1601      ; #HTML(Call <a rel="noopener nofollow"
                       ; href="https://skoolkit.ca/disassemblies/rom/hex/asm/1601.html">CHAN_OPEN</a>.)
; Clear the screen.
 $5DD0 LD HL,$5AFF     ; {Clear the screen and attribute buffers by filling #N$1AFF bytes
 $5DD3 LD DE,$5AFE     ; from #N$5AFE downwards with #N$00.
 $5DD6 LD (HL),$00     ;
 $5DD8 LD BC,$1AFF     ;
 $5DDB LDDR            ; }
; #HTML(Sets the border colour to use when calling
; <a rel="noopener nofollow" href="https://skoolkit.ca/disassemblies/rom/hex/asm/0D6B.html">CLS</a>.)
 $5DDD LD A,$0F        ; {#HTML(Write #INK$0F to
 $5DDF LD ($5C48),A    ; *<a rel="noopener nofollow" href="https://skoolkit.ca/disassemblies/rom/hex/asm/5C48.html">BORDCR</a>.)
                       ; }
 $5DE2 LD IX,$DAC0     ; #REGix=#R$DAC0.
 $5DE6 LD (IX+$23),$00 ; Write #N$00 to *#REGix+#N$23.
 $5DEA LD (IX+$02),$07 ; Write #INK$07 to *#REGix+#N$02 (#R$DAC2).
 $5DEE LD (IX+$22),$FF ; Write #N$FF to *#REGix+#N$22.
 $5DF2 CALL $653D      ; Call #R$653D.
 $5DF5 LD A,$FF        ; {Write #N$FF to *#R$5FBC.
 $5DF7 LD ($5FBC),A    ; }
; Game pause input loop.
@label=Pause_InputLoop
*$5DFA CALL $5FE9      ; Call #R$5FE9.
 $5DFD LD A,$FD        ; {Read from the keyboard;
 $5DFF IN A,($FE)      ; #TABLE(default,centre,centre,centre,centre,centre,centre)
                       ; { =h,r2 Port Number | =h,c5 Bit }
                       ; { =h 0 | =h 1 | =h 2 | =h 3 | =h 4 }
                       ; { #N$FD | A | S | D | F | G }
                       ; TABLE#}
 $5E01 AND %00000010   ; Keep only the bit which relates to the "S" key (bit
                       ; 1).
 $5E03 CALL Z,$6966    ; Call #R$6966 if the "S" key was pressed.
 $5E06 CALL $6992      ; Call #R$6992.
 $5E09 NOP             ; {No operation.
 $5E0A NOP             ; }
 $5E0B XOR A           ; {Write #N$00 to; #LIST
 $5E0C LD ($5FBB),A    ; { *#R$5FBB }
 $5E0F LD ($5FBD),A    ; { *#R$5FBD }
                       ; LIST#}
 $5E12 LD BC,$7FFE     ; {Read from the keyboard;
 $5E15 IN A,(C)        ; #TABLE(default,centre,centre,centre,centre,centre,centre)
                       ; { =h,r2 Port Number | =h,c5 Bit }
                       ; { =h 0 | =h 1 | =h 2 | =h 3 | =h 4 }
                       ; { #N$7F | SPACE | FULL-STOP | M | N | B }
                       ; TABLE#}
 $5E17 RRA             ; {Jump back to #R$5DFA if "SPACE" was not pressed.
 $5E18 JR C,$5DFA      ; }
; The "SPACE" key has been pressed.
 $5E1A LD B,$FE        ; {Read from the keyboard;
 $5E1C IN A,(C)        ; #TABLE(default,centre,centre,centre,centre,centre,centre)
                       ; { =h,r2 Port Number | =h,c5 Bit }
                       ; { =h 0 | =h 1 | =h 2 | =h 3 | =h 4 }
                       ; { #N$FE | SHIFT | Z | X | C | V }
                       ; TABLE#}
 $5E1E RRA             ; {Jump back to #R$5DFA if "CAPS SHIFT" was not pressed.
 $5E1F JR C,$5DFA      ; }
; Both "SPACE" and "CAPS SHIFT" were held down, so reset the game.
 $5E21 EI              ; Enable interrupts.
 $5E22 JR $5DC0        ; Jump to #R$5DC0.

; Populate Room Buffer
;
; Used by the routines at #R$6307, #R$631F, #R$655C, #R$659B and #R$68B8.
; .
; Fetch the current room number and set up the default tile and attribute
; data pointers.
@label=PopulateRoomBuffer
c$5E24 LD A,($5FC5)  ; Load #REGa with *#R$5FC5.
; Initialise the room pointer/ default tile set.
 $5E27 LD HL,$83A9   ; {Write #R$83A9 to *#R$5FC3.
 $5E2A LD ($5FC3),HL ; }
 $5E2D LD HL,$9BAA   ; {Write #R$9BAA to *#R$5FC1.
 $5E30 LD ($5FC1),HL ; }
; If this is not room #N$00, look up the correct room data pointer.
 $5E33 OR A          ; {Call #R$5FC6 if this is not room #N$00.
 $5E34 CALL NZ,$5FC6 ; }
; Clear the screen buffer at #R$C000.
 $5E37 LD HL,$C000   ; {Clear #N$17FF bytes from #R$C000 onwards.
 $5E3A LD DE,$C001   ;
 $5E3D LD (HL),$00   ;
 $5E3F LD BC,$17FF   ;
 $5E42 LDIR          ; }
; Initialise drawing state variables.
 $5E44 LD A,$FF      ; {Write #N$FF to *#R$5FBB.
 $5E46 LD ($5FBB),A  ; }
 $5E49 LD HL,$00FF   ; {Write #N($00FF,$04,$04) to *#R$5FB9.
 $5E4C LD ($5FB9),HL ; }
 $5E4F LD DE,$02C1   ; #REGde=#N$02C1.
; The main room data parsing loop. Reads a command byte from the room
; data and dispatches to the appropriate handler based on its value.
 $5E52 LD HL,($5FC3) ; Fetch the *#R$5FC3 and store it in #REGhl.
; This entry point is used by the routines at #R$5E72, #R$5E92, #R$5EAA and
; #R$5EC8.
@label=PopulateRoomBuffer_ParseByte
*$5E55 LD A,(HL)     ; Fetch the room data byte from the pointer.
 $5E56 OR A          ; {Jump to #R$5E6F if the room data byte is #N$00.
 $5E57 JR Z,$5E6F    ; }
 $5E59 CP $01        ; {Jump to #R$5E92 if the room data byte is #N$01.
 $5E5B JR Z,$5E92    ; }
 $5E5D CP $02        ; {Jump to #R$5EAA if the room data byte is #N$02.
 $5E5F JR Z,$5EAA    ; }
 $5E61 CP $03        ; {Jump to #R$5EDB if the room data byte is #N$03.
 $5E63 JR Z,$5EDB    ; }
 $5E65 CP $04        ; {Jump to #R$5E72 if the room data byte is #N$04.
 $5E67 JR Z,$5E72    ; }
 $5E69 CP $08        ; {Jump to #R$5EC8 if the room data byte is #N$08 or
 $5E6B JR NC,$5EC8   ; higher.}
; This entry point is used by the routines at #R$5E92, #R$5EAA, #R$5EC8 and
; #R$5EDB.
; .
; If none of the above matched, trigger an error (with an arbitary error
; code).
@label=PopulateRoomBuffer_Error
*$5E6D RST $08       ; #HTML(Run the ERROR_1 routine:
                     ; <a rel="noopener nofollow" href="https://skoolkit.ca/disassemblies/rom/hex/asm/0008.html">RST #N$08</a>.)
 $5E6E DEFB $04      ; Error code: #N$04 ("Out of memory").
; Command #N$00: no-op. Skip this byte and continue parsing.
@label=Command00_Skip
*$5E6F INC HL        ; Increment the room data pointer by one.
 $5E70 JR $5E55      ; Jump to #R$5E55.

; Command #N$04: Switch Tile Set
;
; Used by the routine at #R$5E24.
; .
; Command #N$04: Switch the tile set.
; .
; The following byte selects which tile set to use; #N$01 for the default set
; at #R$9BAA, #N$02 for the alternate set at #R$A36A.
;
; DE Tile counter
; HL The room data pointer
@label=Command04_SwitchTileSet
c$5E72 INC HL        ; Increment the room data pointer by one.
 $5E73 LD A,(HL)     ; Fetch the tile set byte.
 $5E74 INC HL        ; Increment the room data pointer by one to move past the command
                     ; byte.
 $5E75 CP $01        ; {Jump to #R$5E88 if the tile set byte is equal to #N$01.
 $5E77 JR Z,$5E88    ; }
 $5E79 CP $02        ; {Jump to #R$5FE7 if the tile set byte is not equal to
 $5E7B JP NZ,$5FE7   ; #N$02.}
; Tile set #N$02: switch to the alternate tile set.
 $5E7E PUSH HL       ; Stash the room data pointer on the stack temporarily.
 $5E7F LD HL,$A36A   ; {Write #R$A36A to *#R$5FC1.
 $5E82 LD ($5FC1),HL ; }
 $5E85 POP HL        ; Restore the room data pointer from the stack.
 $5E86 JR $5E55      ; Jump back to #R$5E55 to continue parsing.
; Tile set #N$01: switch back to the default tile set.
@label=Set_DefaultTileSet
*$5E88 PUSH HL       ; Stash the room data pointer on the stack temporarily.
 $5E89 LD HL,$9BAA   ; {Write #R$9BAA to *#R$5FC1.
 $5E8C LD ($5FC1),HL ; }
 $5E8F POP HL        ; Restore the room data pointer from the stack.
 $5E90 JR $5E55      ; Jump back to #R$5E55 to continue parsing.

; Command #N$01: Skip Tiles
;
; Used by the routine at #R$5E24.
; .
; Command #N$01: skip over a number of tile positions without drawing.
; The following byte specifies how many positions to skip.
;
; DE Tile counter
; HL The room data pointer
@label=Command01_SkipTiles
c$5E92 INC HL        ; Increment the room data pointer by one.
 $5E93 LD A,(HL)     ; Fetch the skip count.
 $5E94 INC HL        ; Increment the room data pointer past the skip count
                     ; byte.
 $5E95 LD B,A        ; Set the skip counter in #REGb.
@label=SkipTiles_Loop
*$5E96 PUSH HL       ; {Stash the room data pointer, tile counter and skip counter on the
 $5E97 PUSH DE       ; stack.
 $5E98 PUSH BC       ; }
 $5E99 LD D,$01      ; #REGd=#N$01 (flag: advance position only, don't draw).
 $5E9B CALL $5F35    ; Call #R$5F35.
 $5E9E POP BC        ; {Restore the skip counter, tile counter and room data pointer from
 $5E9F POP DE        ; the stack.
 $5EA0 POP HL        ; }
 $5EA1 DEC DE        ; Decrease the tile counter by one.
 $5EA2 LD A,E        ; {Jump to #R$5E6D if the tile counter is zero.
 $5EA3 OR D          ;
 $5EA4 JR Z,$5E6D    ; }
 $5EA6 DJNZ $5E96    ; Decrease the skip counter by one and loop back to #R$5E96 until all
                     ; positions are skipped.
 $5EA8 JR $5E55      ; Jump to #R$5E55 to continue parsing.

; Command #N$02: Draw Repeated Tile
;
; Used by the routine at #R$5E24.
; .
; Command #N$02: draw the same tile repeatedly. The following two bytes
; specify the repeat count and the tile ID to draw.
;
; DE Tile counter
; HL Pointer to the room data
@label=Command02_RepeatedTile
c$5EAA INC HL        ; Increment the room data pointer by one.
 $5EAB LD A,(HL)     ; {Fetch the repeat count from *#REGhl and store it in
 $5EAC LD B,A        ; #REGb.}
 $5EAD INC HL        ; Increment the room data pointer by one.
 $5EAE LD A,(HL)     ; Fetch the tile ID from *#REGhl.
 $5EAF INC HL        ; Increment the room data pointer past the command bytes.
@label=RepeatedTile_Loop
*$5EB0 PUSH AF       ; {Stash the tile ID, repeat counter, tile counter and room data
 $5EB1 PUSH BC       ; pointer on the stack.
 $5EB2 PUSH DE       ;
 $5EB3 PUSH HL       ; }
 $5EB4 LD D,$00      ; #REGd=#N$00 (flag: draw tile to room buffer).
 $5EB6 CALL $5F35    ; Call #R$5F35.
 $5EB9 POP HL        ; {Restore the room data pointer, tile counter, repeat counter and tile
 $5EBA POP DE        ; ID from the stack.
 $5EBB POP BC        ;
 $5EBC POP AF        ; }
 $5EBD EX AF,AF'     ; Switch to shadow #REGaf to preserve the tile ID.
 $5EBE DEC DE        ; Decrease the tile counter by one.
 $5EBF LD A,E        ; {Jump back to #R$5E6D if the tile counter is zero.
 $5EC0 OR D          ;
 $5EC1 JR Z,$5E6D    ; }
 $5EC3 EX AF,AF'     ; Switch back to main #REGaf to retrieve the tile ID.
 $5EC4 DJNZ $5EB0    ; Decrease the repeat counter by one and loop back to #R$5EB0 until
                     ; all tiles are drawn.
 $5EC6 JR $5E55      ; Jump to #R$5E55 to continue parsing.

; Command #N$08 (Or Higher): Single Tile
;
; Used by the routine at #R$5E24.
; .
; Command #N$08+: draw a single tile. The command byte itself is the tile
; ID to draw.
;
; DE Tile counter
; HL Pointer to the room data
@label=Command08Plus_SingleTile
c$5EC8 LD A,(HL)     ; Fetch the tile ID from *#REGhl.
 $5EC9 INC HL        ; Increment the room data pointer past the tile ID byte.
 $5ECA PUSH HL       ; {Stash the room data pointer and tile counter on the
 $5ECB PUSH DE       ; stack.}
 $5ECC LD D,$00      ; #REGd=#N$00 (flag: draw tile to room buffer).
 $5ECE CALL $5F35    ; Call #R$5F35.
 $5ED1 POP DE        ; {Restore the tile counter and room data pointer from the
 $5ED2 POP HL        ; stack.}
 $5ED3 DEC DE        ; Decrease the tile counter by one.
 $5ED4 LD A,D        ; {Jump to #R$5E6D if the tile counter is zero.
 $5ED5 OR E          ;
 $5ED6 JR Z,$5E6D    ; }
 $5ED8 JP $5E55      ; Jump to #R$5E55 to continue parsing.

; Command #N$03: Fill Attribute Buffer With Single Colour
;
; Used by the routine at #R$5E24.
; .
; Command #N$03: fill the attribute buffer with a single colour, then
; parse an overlay map to apply per-cell colour changes. The overlay uses a
; simple encoding: #LIST
; { #N$12 + count: skip N attribute cells }
; { #N$1B + count + colour: repeat a colour N times }
; { #N$24: end of attribute data }
; { Any other value: set a single attribute cell directly }
; LIST#
;
; DE Tile counter
; HL Pointer to the room data
@label=Command03_FillAttributes
c$5EDB INC HL        ; Increment the room data pointer by one.
 $5EDC LD C,(HL)     ; Fetch the base fill colour into #REGc.
; Fill #N$02C0 attribute cells at #R$D800 with the base colour.
 $5EDD LD IX,$D800   ; #REGix=#R$D800.
 $5EE1 LD DE,$02C0   ; #REGde=#N($02C0,$04,$04).
@label=FillAttributes_Loop
*$5EE4 LD (IX+$00),C ; Write #REGc to *#REGix+#N$00.
 $5EE7 INC IX        ; Increment #REGix by one.
 $5EE9 DEC DE        ; Decrease #REGde by one.
 $5EEA LD A,E        ; {Jump back to #R$5EE4 until #REGde is zero.
 $5EEB OR D          ;
 $5EEC JR NZ,$5EE4   ; }
; Now parse the attribute overlay data to apply per-cell changes on top
; of the base fill colour.
@label=FillAttributes_Overlay
 $5EEE INC HL        ; Increment #REGhl by one.
 $5EEF LD DE,$02C1   ; #REGde=#N($02C1,$04,$04).
 $5EF2 LD IX,$D800   ; #REGix=#R$D800.
@label=FillAttributes_Overlay_Loop
*$5EF6 LD A,(HL)     ; #REGa=*#REGhl.
 $5EF7 CP $12        ; {Jump to #R$5F12 if #REGa is #N$12 (skip command).
 $5EF9 JR Z,$5F12    ; }
 $5EFB CP $1B        ; {Jump to #R$5F21 if #REGa is #N$1B (repeat command).
 $5EFD JR Z,$5F21    ; }
 $5EFF CP $24        ; {Jump to #R$5F74 if #REGa is #N$24 (end of attribute
 $5F01 JR Z,$5F74    ; data).}
; Otherwise, write this byte directly as a single attribute value.
 $5F03 LD C,(HL)     ; #REGc=*#REGhl.
 $5F04 LD (IX+$00),C ; Write #REGc to *#REGix+#N$00.
 $5F07 DEC DE        ; Decrease #REGde by one.
 $5F08 INC IX        ; Increment #REGix by one.
 $5F0A LD A,E        ; {Jump to #R$5E6D if #REGde is zero.
 $5F0B OR D          ;
 $5F0C JP Z,$5E6D    ; }
 $5F0F INC HL        ; Increment #REGhl by one.
 $5F10 JR $5EF6      ; Jump to #R$5EF6.
; Attribute overlay command #N$12: skip over a number of attribute cells.
; .
; The following byte gives the skip count.
@label=FillAttributes_Skip
*$5F12 INC HL        ; Increment #REGhl by one.
 $5F13 LD B,(HL)     ; Fetch the skip count into #REGb.
@label=FillAttributes_Skip_Loop
*$5F14 INC IX        ; Increment #REGix by one.
 $5F16 DEC DE        ; Decrease #REGde by one.
 $5F17 LD A,E        ; {Jump to #R$5E6D if #REGde is zero.
 $5F18 OR D          ;
 $5F19 JP Z,$5E6D    ; }
 $5F1C DJNZ $5F14    ; Decrease the skip counter by one and loop back to #R$5F14 until
                     ; done.
 $5F1E INC HL        ; Increment #REGhl by one.
 $5F1F JR $5EF6      ; Jump to #R$5EF6.
; Attribute overlay command #N$1B: repeat a colour value a number of
; times. The following two bytes give the repeat count and colour value.
@label=FillAttributes_Repeat
*$5F21 INC HL        ; Increment #REGhl by one.
 $5F22 LD B,(HL)     ; Fetch the repeat count into #REGb.
 $5F23 INC HL        ; Increment #REGhl by one.
 $5F24 LD C,(HL)     ; Fetch the colour value into #REGc.
 $5F25 INC HL        ; Increment #REGhl past the colour value byte.
@label=FillAttributes_Repeat_Loop
*$5F26 LD (IX+$00),C ; Write #REGc to *#REGix+#N$00.
 $5F29 INC IX        ; Increment #REGix by one.
 $5F2B DEC DE        ; Decrease #REGde by one.
 $5F2C LD A,E        ; {Jump to #R$5E6D if #REGde is zero.
 $5F2D OR D          ;
 $5F2E JP Z,$5E6D    ; }
 $5F31 DJNZ $5F26    ; Decrease the repeat counter by one and loop back to #R$5F26 until
                     ; done.
 $5F33 JR $5EF6      ; Jump to #R$5EF6.

; Draw Room Tile
;
; Used by the routines at #R$5E92, #R$5EAA and #R$5EC8.
; .
; Draws a single tile to the room buffer at the current drawing
; position stored in *#R$5FB9. The position is advanced after each call. If
; #REGd bit 0 is set, the position is advanced but no tile is drawn (used for
; blank/ skip tiles in command #N$01).
;
; A Tile character to draw
; D Bit 0: #N$01=advance position only, #N$00=draw and advance
;
; Load and advance the current column/row drawing position.
@label=DrawRoomTile
c$5F35 LD BC,($5FB9) ; #REGbc=*#R$5FB9.
 $5F39 INC C         ; Increment the column in #REGc.
 $5F3A EX AF,AF'     ; Stash the tile character in shadow #REGaf.
; If the column has reached #N$20 (past the right edge), wrap to the next
; row.
 $5F3B BIT 5,C       ; {Jump to #R$5F41 if bit 5 of #REGc is set.
 $5F3D JR NZ,$5F41   ; }
 $5F3F JR $5F44      ; Jump to #R$5F44.
@label=DrawRoomTile_NextRow
*$5F41 INC B         ; Increment the row in #REGb.
 $5F42 LD C,$00      ; Reset the column to #N$00.
@label=DrawRoomTile_StorePosition
*$5F44 LD ($5FB9),BC ; Write #REGbc to *#R$5FB9.
 $5F48 BIT 0,D       ; {Return if this is a position-advance-only call (bit 0 of #REGd is
 $5F4A RET NZ        ; set).}
; Calculate the room buffer address from the column (#REGc) and row
; (#REGb) drawing position. This converts the character cell co-ordinates into
; a pixel address within the room buffer at #R$C000.
 $5F4B LD L,C        ; #REGl=#REGc.
 $5F4C LD A,B        ; #REGa=#REGb.
 $5F4D AND %00000111 ; Keep only bits 0-2.
 $5F4F RRCA          ; {Rotate right three positions to move bits 0-2 into bits
 $5F50 RRCA          ; 5-7.
 $5F51 RRCA          ; }
 $5F52 OR L          ; Merge in the column bits from #REGl.
 $5F53 LD L,A        ; #REGl=#REGa.
 $5F54 LD A,B        ; #REGa=#REGb.
 $5F55 AND %00011000 ; Keep only bits 3-4.
 $5F57 OR %11000000  ; Set bits 6-7 for the room buffer base at #R$C000.
 $5F59 LD H,A        ; #REGh=#REGa.
; Look up the tile graphic data from the active tile set and copy all
; #N$08 pixel rows to the room buffer.
 $5F5A PUSH HL       ; Stash the room buffer address on the stack.
 $5F5B LD DE,($5FC1) ; #REGde=*#R$5FC1 (active tile set base address).
 $5F5F EX AF,AF'     ; Retrieve the tile character from shadow #REGaf.
 $5F60 SUB $08       ; #REGa-=#N$08.
 $5F62 LD L,A        ; {Transfer the tile index to #REGhl.
 $5F63 LD H,$00      ; }
 $5F65 ADD HL,HL     ; {Multiply by #N$08 (bytes per tile).
 $5F66 ADD HL,HL     ;
 $5F67 ADD HL,HL     ; }
 $5F68 ADD HL,DE     ; Add the tile set base address.
 $5F69 EX DE,HL      ; Exchange so #REGde=tile graphic data.
 $5F6A POP HL        ; Restore the room buffer address from the stack.
 $5F6B LD B,$08      ; Set a line counter in #REGb of #N$08.
@label=DrawRoomTile_CopyLoop
*$5F6D LD A,(DE)     ; {Copy the tile graphic data byte to the room buffer.
 $5F6E LD (HL),A     ; }
 $5F6F INC DE        ; Advance the tile graphic data pointer.
 $5F70 INC H         ; Move down one pixel row in the room buffer.
 $5F71 DJNZ $5F6D    ; Decrease the line counter by one and loop back to #R$5F6D until all
                     ; #N$08 rows are drawn.
 $5F73 RET           ; Return.
; This entry point is used by the routine at #R$5EDB.
; .
; Attribute overlay command #N$24: end of attribute data. Finalises the
; room setup by resetting game flags and calculating the base address for this
; room's colour attribute lookup table.
@label=FillAttributes_End
*$5F74 LD A,($5FA9)  ; {Jump to #R$5F83 if *#R$5FA9 is zero.
 $5F77 OR A          ;
 $5F78 JR Z,$5F83    ; }
 $5F7A XOR A         ; {Write #N$00 to *#R$5FA9.
 $5F7B LD ($5FA9),A  ; }
 $5F7E NOP           ; No operation.
 $5F7F NOP           ; No operation.
 $5F80 LD ($DAE3),A  ; Write #N$00 to *#R$DAE3.
; Calculate the base address for this room's colour attributes. Each room
; has #N$20 bytes of attribute data stored sequentially from #R$DE9E, so
; multiply the zero-indexed room number by #N$20 and add the base.
@label=FillAttributes_SetAttributeBase
*$5F83 LD A,($5FC5)  ; #REGa=*#R$5FC5.
 $5F86 DEC A         ; Decrease by one to make zero-indexed.
 $5F87 LD DE,$DE9E   ; #REGde=#R$DE9E.
 $5F8A LD L,A        ; {Transfer the room index to #REGhl.
 $5F8B LD H,$00      ; }
 $5F8D ADD HL,HL     ; {Multiply by #N$20.
 $5F8E ADD HL,HL     ;
 $5F8F ADD HL,HL     ;
 $5F90 ADD HL,HL     ;
 $5F91 ADD HL,HL     ; }
 $5F92 ADD HL,DE     ; Add the base address.
 $5F93 LD ($5FB5),HL ; Write the result to *#R$5FB5.
; Wait for the next interrupt frame, then transfer the completed room
; buffer to the display.
 $5F96 EI            ; Enable interrupts.
 $5F97 HALT          ; Halt operation (suspend CPU until the next interrupt).
 $5F98 DI            ; Disable interrupts.
; Set up printing the room to the screen buffer.
 $5F99 LD B,$16      ; Set a counter in #REGb for #N$16 rows to print.
 $5F9B LD HL,$C000   ; Point #REGhl at #R$C000.
 $5F9E JP $6438      ; Jump to #R$6438.

