; Set Room Data Pointer
;
; Used by the routine at #R$5E24.
; .
; Sets the room data pointer for the current room.
; .
; The room data is stored as a series of blocks from #R$83AA, separated by
; #N$00 terminators. This routine scans through the data to find the block
; matching the *#R$5FC5.
@label=FindRoomData
c$5FC6 LD A,($5FC5)  ; {#REGd=*#R$5FC5.
 $5FC9 LD D,A        ; }
 $5FCA LD E,$01      ; #REGe=#N$01 (start searching from room #N$01).
 $5FCC CP E          ; {Return if the current room is #N$01 (the data pointer already
 $5FCD RET Z         ; defaults to the first room).}
; Scan through the room data blocks. Each block is terminated by #N$00,
; so count terminators to find the Nth room.
 $5FCE LD HL,$83AA   ; #REGhl=#R$83AA (start of room data).
@label=FindRoomData_ScanLoop
*$5FD1 LD A,(HL)     ; #REGa=*#REGhl.
 $5FD2 INC HL        ; Advance the data pointer.
 $5FD3 OR A          ; {Jump to #R$5FD8 if a #N$00 terminator was found.
 $5FD4 JR Z,$5FD8    ; }
 $5FD6 JR $5FD1      ; Jump to #R$5FD1 to keep scanning.
; Found a block terminator so check if we've reached the target room.
@label=FindRoomData_FoundTerminator
*$5FD8 INC E         ; Increment the room counter in #REGe.
 $5FD9 LD A,E        ; {Jump to #R$5FE7 if the room counter has exceeded #N$11 (maximum
 $5FDA CP $11        ; number of rooms/room not found).
 $5FDC JR NC,$5FE7   ; }
 $5FDE CP D          ; {Jump to #R$5FE3 if the room counter matches the target
 $5FDF JR Z,$5FE3    ; room number.}
 $5FE1 JR $5FD1      ; Otherwise, jump to #R$5FD1 to keep scanning.
; The target room was found so store the pointer to its data.
@label=FindRoomData_Found
*$5FE3 LD ($5FC3),HL ; Write #REGhl to *#R$5FC3.
 $5FE6 RET           ; Return.

; Raise Table Error
;
; Used by the routines at #R$5E72 and #R$5FC6.
; .
; #HTML(<code>RST #N$08</code> with the "Invalid argument" error code.)
; .
; Used when the table index >= #N$11 in #R$5FC6, and when an invalid tile set
; is requested in #R$5E72.
@label=Raise_Table_Error
c$5FE7 RST $08       ; #HTML(Run the ERROR_1 routine:
                     ; <a rel="noopener nofollow" href="https://skoolkit.ca/disassemblies/rom/hex/asm/0008.html">RST #N$08</a>.)
 $5FE8 DEFB $0A      ; Error code: #N$0A ("Invalid argument").

; Main Game Loop
;
; Used by the routine at #R$5DC0.
; .
; Main game loop handler. Reads player input (either from Kempston
; joystick or keyboard), moves Percy, checks for collisions with the room
; scenery, handles room transitions, item collection and updates Percy's
; animation frame.
@label=MainGameLoop
c$5FE9 LD A,($5FA7)    ; {If *#R$5FA7 is unset, call #R$653D.
 $5FEC OR A            ;
 $5FED CALL Z,$6480    ; }
; Seed the chick animation states and introduce a short random delay
; using the Memory Refresh Register.
 $5FF0 LD A,R          ; {#REGl=the contents of the Memory Refresh Register.
 $5FF2 LD L,A          ; }
 $5FF3 LD H,$00        ; Set the low byte in #REGh to #N$00 so only low memory
                       ; is accessed.
 $5FF5 LD A,(HL)       ; {Fetch a byte from low memory and write it to
 $5FF6 LD ($5FB4),A    ; *#R$5FB4.}
 $5FF9 AND %00000111   ; Keep only bits 0-2.
 $5FFB LD B,A          ; Set the same byte value as a delay counter in #REGb.
@label=MainGameLoop_RandomDelay
*$5FFC NOP             ; No operation.
 $5FFD DJNZ $5FFC      ; Decrease the delay counter by one and loop back to #R$5FFC until
                       ; done.
; Read the Kempston joystick port to check for input.
@label=MainGameLoop_ReadInput
 $5FFF LD BC,$EFFE     ; #REGbc=#N$EFFE.
 $6002 LD IX,$DAC0     ; #REGix=#R$DAC0.
 $6006 LD A,$1F        ; {Write #N$1F to *#R$5FA2 (default: no direction
 $6008 LD ($5FA2),A    ; pressed).}
 $600B IN A,(C)        ; Read from the Kempston joystick port.
 $600D AND %00011111   ; Keep only bits 0-4.
 $600F CP $1F          ; {Jump to #R$6015 if any joystick direction was
 $6011 JR NZ,$6015     ; detected.}
 $6013 JR $601B        ; Jump to #R$601B.
@label=MainGameLoop_JoystickInput
*$6015 LD ($5FA2),A    ; Write #REGa to *#R$5FA2.
 $6018 JP $60A8        ; Jump to #R$60A8.
; Check whether to read from Kempston joystick or keyboard. Bit 7 of
; *#R$5FA6 indicates keyboard mode.
@label=MainGameLoop_CheckInputMode
*$601B LD A,($5FA6)    ; #REGa=*#R$5FA6.
 $601E BIT 7,A         ; {Jump to #R$606B if keyboard mode is active (bit 7 is
 $6020 JR NZ,$606B     ; set).}
 $6022 LD BC,$001F     ; #REGbc=#N($001F,$04,$04).
 $6025 OR A            ; {Jump to #R$6043 if #REGa is non-zero.
 $6026 JR NZ,$6043     ; }
; Auto-detect input method. Poll the joystick port repeatedly and if any
; input is detected, switch to joystick mode; otherwise switch to keyboard.
 $6028 LD E,$F0        ; #REGe=#N$F0 (poll retry counter).
@label=MainGameLoop_AutoDetect_Loop
*$602A IN A,(C)        ; Read from the joystick port.
 $602C AND %00011111   ; Keep only bits 0-4.
 $602E CP $1F          ; {Jump to #R$603C if no joystick was input detected.
 $6030 JR Z,$603C      ; }
 $6032 DEC E           ; Decrease the retry counter.
 $6033 JR NZ,$602A     ; Jump to #R$602A until all the retries have been used.
; Joystick input was detected so set joystick mode.
 $6035 LD A,$1F        ; {Write #N$1F to *#R$5FA6 (joystick mode).
 $6037 LD ($5FA6),A    ; }
 $603A JR $6043        ; Jump to #R$6043.
; No joystick was detected so set keyboard mode.
@label=MainGameLoop_SetKeyboardMode
*$603C LD A,$80        ; {Write #N$80 to *#R$5FA6 (keyboard mode).
 $603E LD ($5FA6),A    ; }
 $6041 JR $606B        ; Jump to #R$606B.
; Read the Kempston joystick and remap the direction bits into the
; game's internal input format stored in *#R$5FA2.
; .
; #TABLE(default,centre,centre,centre)
; { =h Joystick Bit | =h Direction | =h Game Bit }
; { 4 | Fire | 0 }
; { 0 | Right | 3 }
; { 1 | Left | 4 }
; { 2 | Down | 2 }
; { 3 | Up | 1 }
; TABLE#
@label=MainGameLoop_ReadJoystick
*$6043 IN A,(C)        ; Read from the joystick port.
 $6045 LD E,$1F        ; #REGe=#N$1F (all directions inactive).
@label=MainGameLoop_Joystick_TestFire
 $6047 BIT 4,A         ; {Jump to #R$604D if fire is not being pressed.
 $6049 JR Z,$604D      ; }
 $604B RES 0,E         ; Reset bit 0 of #REGe (fire active).
@label=MainGameLoop_Joystick_TestRight
*$604D BIT 0,A         ; {Jump to #R$6053 if right is not being pressed.
 $604F JR Z,$6053      ; }
 $6051 RES 3,E         ; Reset bit 3 of #REGe (right active).
@label=MainGameLoop_Joystick_TestLeft
*$6053 BIT 1,A         ; {Jump to #R$6059 if left is not being pressed.
 $6055 JR Z,$6059      ; }
 $6057 RES 4,E         ; Reset bit 4 of #REGe (left active).
@label=MainGameLoop_Joystick_TestDown
*$6059 BIT 2,A         ; {Jump to #R$605F if down is not being pressed.
 $605B JR Z,$605F      ; }
 $605D RES 2,E         ; Reset bit 2 of #REGe (down active).
@label=MainGameLoop_Joystick_TestUp
*$605F BIT 3,A         ; {Jump to #R$6065 if up is not being pressed.
 $6061 JR Z,$6065      ; }
 $6063 RES 1,E         ; Reset bit 1 of #REGe (up active).
@label=MainGameLoop_Joystick_Store
*$6065 LD A,E          ; {Write #REGe to *#R$5FA2.
 $6066 LD ($5FA2),A    ; }
 $6069 JR $60A8        ; Jump to #R$60A8.
; Read keyboard input. Scans multiple keyboard half-rows and maps them
; into the same internal input format.
; .
; #TABLE(default,centre,centre,centre)
; { =h Port | =h Keys | =h Game Bit }
; { #N$7FFE | SPACE..B | 0 (fire) }
; { #N$BFFE | ENTER..H | 2 (down) }
; { #N$DFFE | P..Y | 1 (up) }
; { #N$FBFE | Q (bit 0) | 4 (left) }
; { #N$FBFE | W (bit 1) | 3 (right) }
; TABLE#
@label=MainGameLoop_ReadKeyboard
*$606B LD BC,$7FFE     ; #REGbc=#N$7FFE.
 $606E LD E,$1F        ; #REGe=#N$1F (all directions inactive).
; Read SPACE-B row for fire.
 $6070 IN A,(C)        ; Read from the keyboard port.
 $6072 AND %00011111   ; Keep only bits 0-4.
 $6074 CP $1F          ; {Jump to #R$607A if no key was being pressed.
 $6076 JR Z,$607A      ; }
 $6078 RES 0,E         ; Reset bit 0 of #REGe (fire active).
; Read ENTER-H row for down.
@label=MainGameLoop_Keyboard_TestDown
*$607A LD B,$BF        ; #REGb=#N$BF.
 $607C IN A,(C)        ; Read from the keyboard port.
 $607E AND %00011111   ; {Jump to #R$6086 if no key was being pressed.
 $6080 CP $1F          ;
 $6082 JR Z,$6086      ; }
 $6084 RES 2,E         ; Reset bit 2 of #REGe (down active).
; Read P-Y row for up.
@label=MainGameLoop_Keyboard_TestUp
*$6086 LD B,$DF        ; #REGb=#N$DF.
 $6088 IN A,(C)        ; Read from the keyboard port.
 $608A AND %00011111   ; {Jump to #R$6092 if no key was being pressed.
 $608C CP $1F          ;
 $608E JR Z,$6092      ; }
 $6090 RES 1,E         ; Reset bit 1 of #REGe (up active).
; Read Q-T row for left (Q, bit 0) and right (W, bit 1).
@label=MainGameLoop_Keyboard_TestLeftRight
*$6092 LD B,$FB        ; #REGb=#N$FB.
 $6094 IN A,(C)        ; Read from the keyboard port.
 $6096 AND %00000001   ; {Jump to #R$609C if Q is not being pressed.
 $6098 JR NZ,$609C     ; }
 $609A RES 4,E         ; Reset bit 4 of #REGe (left active).
@label=MainGameLoop_Keyboard_TestRight
*$609C IN A,(C)        ; Read from the keyboard port again.
 $609E AND %00000010   ; {Jump to #R$60A4 if W is not being pressed.
 $60A0 JR NZ,$60A4     ; }
 $60A2 RES 3,E         ; Reset bit 3 of #REGe (right active).
@label=MainGameLoop_Keyboard_Store
*$60A4 LD A,E          ; {Write #REGe to *#R$5FA2.
 $60A5 LD ($5FA2),A    ; }
; Handle Percy's egg drop state. If *#R$5FA9 is set, Percy has dropped
; an egg and it is currently in flight.
@label=MainGameLoop_HandleEggDrop
*$60A8 LD A,($5FA9)    ; {Jump to #R$60D9 if *#R$5FA9 is unset (no egg active).
 $60AB OR A            ;
 $60AC JR Z,$60D9      ; }
; Egg is in flight so advance its Y position.
 $60AE LD A,(IX+$21)   ; #REGa=*#REGix+#N$21.
 $60B1 ADD A,$04       ; #REGa+=#N$04.
 $60B3 CP $A8          ; Has the egg reached Y position #N$A8?
 $60B5 JR C,$60BF      ; Jump to #R$60BF if not yet reached.
; Egg has reached its target so end the egg drop state.
 $60B7 LD (IX+$23),$00 ; Write #N$00 to *#REGix+#N$23.
 $60BB XOR A           ; {Write #N$00 to *#R$5FA9 (egg drop complete).
 $60BC LD ($5FA9),A    ; }
@label=MainGameLoop_UpdateEggY
*$60BF LD (IX+$21),A   ; Write #REGa to *#REGix+#N$21.
; Play a rising sound effect while the egg is in flight.
 $60C2 PUSH IX         ; Stash #REGix on the stack.
 $60C4 LD HL,($5FB7)   ; #REGhl=*#R$5FB7.
 $60C7 LD BC,$0010     ; {#REGhl+=#N$0010.
 $60CA ADD HL,BC       ; }
 $60CB LD ($5FB7),HL   ; Write #REGhl to *#R$5FB7.
 $60CE LD DE,$0004     ; #REGde=#N($0004,$04,$04).
 $60D1 CALL $03B5      ; #HTML(Call <a rel="noopener nofollow"
                       ; href="https://skoolkid.github.io/rom/asm/03B5.html">BEEP</a>.)
 $60D4 DI              ; Disable interrupts.
 $60D5 POP IX          ; Restore #REGix from the stack.
 $60D7 JR $610E        ; Jump to #R$610E.
; Check if fire is pressed and conditions are met to drop an egg.
@label=MainGameLoop_TestFirePressed
*$60D9 LD A,($5FA2)    ; {Jump to #R$610E if *#R$5FA2 states that fire has not
 $60DC BIT 0,A         ; been pressed.
 $60DE JR NZ,$610E     ; }
 $60E0 LD A,($5FAD)    ; {Jump to #R$610E if *#R$5FAD is set.
 $60E3 OR A            ;
 $60E4 JR NZ,$610E     ; }
; Percy must be below Y position #N$84 to drop an egg.
 $60E6 LD A,(IX+$01)   ; {Jump to #R$610E if Percy's Y position (*#REGix+#N$01) is greater
 $60E9 CP $84          ; than or equal to #N$84 (too high to drop an egg).
 $60EB JR NC,$610E     ; }
; Start the egg drop sequence and set the egg's target position/
; activate the egg state.
 $60ED ADD A,$10       ; #REGa+=#N$10.
 $60EF PUSH AF         ; Stash #REGaf on the stack.
 $60F0 CALL $679C      ; Call #R$679C.
 $60F3 POP AF          ; Restore #REGaf from the stack.
 $60F4 LD (IX+$21),A   ; Write #REGa to *#REGix+#N$21 (egg target Y).
 $60F7 LD (IX+$23),$0F ; Write #N$0F to *#REGix+#N$23.
 $60FB LD A,(IX+$00)   ; #REGa=*#REGix+#N$00 (Percy's X position).
 $60FE ADD A,$05       ; #REGa+=#N$05.
 $6100 LD (IX+$20),A   ; Write #REGa to *#REGix+#N$20 (egg target X).
 $6103 OR %11111111    ; #REGa=#N$FF.
 $6105 LD ($5FA9),A    ; Write #N$FF to *#R$5FA9 (egg drop in progress).
