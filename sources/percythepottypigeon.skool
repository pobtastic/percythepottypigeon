 $6277 LD (IX+$00),C   ; Write #REGc to *#REGix+#N$00 (Percy's X position).
 $627A LD (IX+$01),B   ; Write #REGb to *#REGix+#N$01 (Percy's Y position).
 $627D JP $6338        ; Jump to #R$6338.

; Move Percy Left
;
; Used by the routine at #R$5FE9.
; .
; Move Percy left. Checks screen boundary and handles room transitions.
;
; IX Pointer to Percys state data
; D Number of pixels to move
@label=MovePercyLeft
c$6280 LD A,$01      ; {Write #N$01 to *#R$5FA5 (Percy is facing left).
 $6282 LD ($5FA5),A  ; }
; If *#R$5FAA is set, apply an extra boundary check.
 $6285 LD A,($5FAA)  ; {Jump to #R$6297 if *#R$5FAA is set.
 $6288 OR A          ;
 $6289 JR Z,$6297    ; }
 $628B LD A,(IX+$00) ; Load #REGa with Percy's X position (*#REGix+#N$00) -
                     ; #N$05.
 $628E SUB $05       ;
; If Percy has gone past the left edge transition into the previous room.
 $6290 JP C,$631F    ; Jump to #R$631F if Percy's new position is less than #N$00
                     ; (transition to the previous room).
 $6293 SUB D         ; Subtract the movement speed from Percy's X position.
 $6294 JP C,$631F    ; Jump to #R$631F if Percy's new position is less than #N$00
                     ; (transition to the previous room).
@label=MovePercyLeft_Apply
*$6297 LD A,(IX+$00) ; {Load #REGa with Percy's X position (*#REGix+#N$00) - the number of
 $629A SUB D         ; pixels to move (from #REGd).}
; If Percy has gone past the left edge transition into the previous room.
 $629B JP C,$631F    ; Jump to #R$631F if Percy's new position is less than #N$00
                     ; (transition to the previous room).
 $629E LD (IX+$00),A ; Write Percy's updated X screen position back to
                     ; *#REGix+#N$00.
 $62A1 RET           ; Return.

; Move Percy Right
;
; Used by the routine at #R$5FE9.
; .
; Move Percy right. Checks screen boundary and handles room transitions.
;
; IX Pointer to Percys state data
; D Number of pixels to move
@label=MovePercyRight
c$62A2 XOR A         ; {Write #N$00 to *#R$5FA5 (Percy is facing right).
 $62A3 LD ($5FA5),A  ; }
