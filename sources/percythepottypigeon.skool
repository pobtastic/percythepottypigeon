; Calculate the room attribute buffer address from Percy's position.
@label=MainGameLoop_CalcAttributeAddress
*$619E LD A,C          ; #REGa=#REGc.
 $619F RRCA            ; {Rotate right three positions (divide X by #N$08).
 $61A0 RRCA            ;
 $61A1 RRCA            ; }
 $61A2 AND %00011111   ; Keep only bits 0-4 (character column).
 $61A4 LD L,A          ; #REGl=#REGa.
 $61A5 LD A,B          ; {#REGa=#REGb, keep only bits 0-2 (sub-character Y
 $61A6 AND $07         ; offset).}
 $61A8 OR A            ; {Jump to #R$61AD if #REGa is zero (aligned to
 $61A9 JR Z,$61AD      ; character row).}
 $61AB SET 1,D         ; Set bit 1 of #REGd (not Y-aligned).
@label=MainGameLoop_CalcAttributeAddress_2
*$61AD LD A,B          ; #REGa=#REGb.
 $61AE RLCA            ; {Rotate left two positions.
 $61AF RLCA            ; }
 $61B0 AND %11100000   ; Keep only bits 5-7.
 $61B2 OR L            ; Merge in the column from #REGl.
 $61B3 LD L,A          ; #REGl=#REGa.
 $61B4 LD A,B          ; #REGa=#REGb.
 $61B5 RLCA            ; {Rotate left two positions.
 $61B6 RLCA            ; }
 $61B7 AND %00000011   ; Keep only bits 0-1.
 $61B9 OR %11011000    ; Set bits 3-4 and 6-7 for room attribute buffer at
                       ; #R$D800.
 $61BB LD H,A          ; #REGh=#REGa.
; Check a vertical strip of attribute cells for solid tiles. The height
; of the strip depends on whether Percy is Y-aligned.
 $61BC LD B,$02        ; #REGb=#N$02 (rows to check).
 $61BE BIT 1,D         ; {Jump to #R$61C3 if Y-aligned.
 $61C0 JR Z,$61C3      ; }
 $61C2 INC B           ; Check an extra row if not Y-aligned.
@label=MainGameLoop_CollisionCheck_Loop
*$61C3 PUSH BC         ; Stash the row counter on the stack.
; Check the current attribute cell and the one to its right.
 $61C4 LD A,(HL)       ; #REGa=*#REGhl.
 $61C5 AND E           ; Mask with #REGe.
 $61C6 CP E            ; {Jump to #R$6244 if a collision was detected (not all
 $61C7 JR NZ,$6244     ; bits set).}
 $61C9 INC HL          ; Move to the next column.
 $61CA LD A,(HL)       ; #REGa=*#REGhl.
 $61CB AND E           ; Mask with #REGe.
 $61CC CP E            ; {Jump to #R$6244 if a collision was detected.
 $61CD JR NZ,$6244     ; }
 $61CF DEC HL          ; Move back to the original column.
; If Percy is not X-aligned, also check the cell two columns right.
 $61D0 BIT 0,D         ; {Jump to #R$61DD if X-aligned.
 $61D2 JR Z,$61DD      ; }
 $61D4 INC HL          ; {Move two columns right.
 $61D5 INC HL          ; }
 $61D6 LD A,(HL)       ; #REGa=*#REGhl.
 $61D7 AND E           ; Mask with #REGe.
 $61D8 CP E            ; {Jump to #R$6244 if a collision was detected.
 $61D9 JR NZ,$6244     ; }
 $61DB DEC HL          ; {Move back two columns.
 $61DC DEC HL          ; }
; Move down one row in the attribute buffer and continue.
@label=MainGameLoop_CollisionCheck_NextRow
*$61DD LD BC,$0020     ; {#REGhl+=#N($0020,$04,$04).
 $61E0 ADD HL,BC       ; }
 $61E1 POP BC          ; Restore the row counter from the stack.
 $61E2 DJNZ $61C3      ; Decrease the row counter by one and loop back to #R$61C3 until all
                       ; rows are checked.
