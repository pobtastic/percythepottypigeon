 $6AE5 INC C           ; {Increment #REGc by #N$06.
 $6AE6 INC C           ;
 $6AE7 INC C           ;
 $6AE8 INC C           ;
 $6AE9 INC C           ;
 $6AEA INC C           ; }
 $6AEB JR $6AD0        ; Jump to #R$6AD0 to try again.
; Flying right so shift the starting X position leftward.
@label=HandleRedBird_ShiftLeft
