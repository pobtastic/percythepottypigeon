 $622A LD A,(IX+$01)   ; #REGa=*#REGix+#N$01.
 $622D AND %11111000   ; Keep only bits 3-7 (snap to character row).
 $622F LD (IX+$01),A   ; Write #REGa to *#REGix+#N$01.
 $6232 OR %11111111    ; {Write #N$FF to *#R$5FAD (landed on platform flag).
 $6234 LD ($5FAD),A    ; }
 $6237 JR $6241        ; Jump to #R$6241.
; Check if up is pressed while on a platform.
@label=MainGameLoop_CheckGround_TestUp
