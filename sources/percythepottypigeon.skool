 $623B JR NZ,$6241     ; }
; No platform below so clear the landed flag.
@label=MainGameLoop_ClearLandedFlag
*$623D XOR A           ; {Write #N$00 to *#R$5FAD.
 $623E LD ($5FAD),A    ; }
@label=MainGameLoop_UpdateAnimation
