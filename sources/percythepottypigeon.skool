; Handle Level Complete
;
; Used by the routine at #R$66E1.
; .
; Called after a worm is delivered to check if all worms have been
; collected. Decrements the worms remaining counter and if zero, plays a
; completion jingle, advances to the next room and sets up the new level.
@label=Handle_Level_Complete
c$68B8 LD HL,$5FB2   ; {Decrease *#R$5FB2 by one.
 $68BB DEC (HL)      ; }
 $68BC RET NZ        ; Return if the worms remaining counter is not zero.
 $68BD PUSH IX       ; Stash #REGix on the stack.
 $68BF LD B,$28      ; Set a sound step counter in #REGb of #N$28 steps.
 $68C1 LD HL,$0320   ; #REGhl=#N$0320 (initial sound pitch).
 $68C4 LD DE,$0008   ; #REGde=#N$0008 (pitch step per iteration).
; Play the level complete sound effect.
@label=Level_Complete_Sound_Loop
*$68C7 CALL $693B    ; Call #R$693B.
 $68CA DJNZ $68C7    ; Decrease the step counter by one and loop back to
                     ; #R$68C7 until the sound is complete.
 $68CC LD A,$0F      ; {#HTML(Write #N$0F to; #LIST
 $68CE LD ($5C48),A  ; { *<a rel="noopener nofollow" href="https://skoolkit.ca/disassemblies/rom/hex/asm/5C48.html">BORDCR</a> }
 $68D1 LD ($5FBD),A  ; { *#R$5FBD }
                     ; LIST#)}
 $68D4 LD A,$01      ; {Set border to #INK$01.
 $68D6 OUT ($FE),A   ; }
 $68D8 POP IX        ; Restore #REGix from the stack.
 $68DA CALL $68F7    ; Call #R$68F7.
; Locate the level splash screen.
 $68DD LD A,($5FB1)  ; Fetch the current level from *#R$5FB1.
 $68E0 ADD A,$0B     ; Add #N$0B (the total number of rooms) to calculate the pointer to
                     ; the level splash screen.
 $68E2 LD ($5FC5),A  ; Write the result to *#R$5FC5.
 $68E5 CALL $5E24    ; Call #R$5E24.
; #HTML(#AUDIO(lose-life.wav)(#INCLUDE(LoseLife)))
 $68E8 LD HL,$FBF9   ; #REGhl=#R$FBF9.
 $68EB CALL $FAC4    ; Call #R$FAC4.
 $68EE DI            ; Disable interrupts.
 $68EF CALL $6562    ; Call #R$6562.
 $68F2 XOR A         ; {Write #N$00 to *#R$5FA5 (Percy is facing right).
 $68F3 LD ($5FA5),A  ; }
 $68F6 RET           ; Return.

; Set Up Next Level
;
; Used by the routines at #R$653D and #R$68B8.
; .
; Prepares the game state for the next level. Increments the current
; level number, awards an extra life on level #N$04, loads the worm collection
; target for the new level from the lookup table, and resets all collected and
; disabled worm markers in the room attribute data.
;
; .
;
; Increment the level counter and check for extra life award.
@label=SetUp_NextLevel
c$68F7 LD A,($5FB1)  ; Fetch the current level from *#R$5FB1.
 $68FA LD HL,$5FB3   ; Point #REGhl at #R$5FB3.
 $68FD CP $05        ; {Jump to #R$6911 if the current level is equal to #N$05 (don't
 $68FF JR Z,$6911    ; increment past the maximum).}
 $6901 INC A         ; {Increment the level number and write it back to
 $6902 LD ($5FB1),A  ; *#R$5FB1.}
 $6905 CP $04        ; {Jump to #R$6911 if the new level is not equal to #N$04.
 $6907 JR NZ,$6911   ; }
; Award an extra life on level #N$04.
 $6909 INC (HL)      ; {Increment the lives display character at *#REGhl.
 $690A LD A,(HL)     ; }
 $690B LD HL,$50F0   ; Point #REGhl at #N$50F0 (screen buffer position for the lives
                     ; display).
 $690E CALL $6581    ; Call #R$6A5F.
; Load the worm collection target for the new level.
@label=SetWorm_Target
*$6911 LD A,($5FB1)  ; Fetch the current level from *#R$5FB1.
 $6914 LD B,$00      ; {Set #REGbc to the level number minus one as a table
 $6916 LD C,A        ; index.
 $6917 DEC C         ; }
 $6918 LD HL,$6961   ; Point #REGhl at #R$6961.
 $691B ADD HL,BC     ; Add the index to get the table entry address.
 $691C LD A,(HL)     ; {Write the worm collection count for this level to
 $691D LD HL,$5FB2   ; *#R$5FB2.
 $6920 LD (HL),A     ; }
; Reset all collected and disabled worm markers in the room attribute
; data back to #N$00.
 $6921 LD HL,$DE9E   ; Point #REGhl at #R$DE9E.
 $6924 LD BC,$0160   ; Set the byte counter to #N$0160 in #REGbc.
@label=ResetWorm_Markers_Loop
*$6927 LD A,(HL)     ; {If *#REGhl equals #N$1F (disabled marker), write #N$00
 $6928 CP $1F        ; to *#REGhl.
 $692A JR NZ,$692E   ;
 $692C LD (HL),$00   ; }
@label=ResetWorm_TestCollected
*$692E CP $1E        ; {If *#REGhl equals #N$1E (collected marker), write #N$00
 $6930 JR NZ,$6934   ; to *#REGhl.
 $6932 LD (HL),$00   ; }
@label=ResetWorm_Markers_Next
*$6934 INC HL        ; Move to the next data byte.
 $6935 DEC BC        ; Decrease the byte counter by one.
 $6936 LD A,C        ; {Jump back to #R$6927 until all #N$0160 bytes are
 $6937 OR B          ; processed.
 $6938 JR NZ,$6927   ; }
 $693A RET           ; Return.

; Level Complete Sound Step
;
; Used by the routine at #R$68B8.
;
; B Sound step counter
; HL Current pitch
; DE Pitch adjustment per step
@label=LevelCompleteSoundStep
c$693B PUSH BC       ; {Stash the step counter, pitch and pitch adjustment on
 $693C PUSH HL       ; the stack.
 $693D PUSH DE       ; }
 $693E LD A,$19      ; {Call #R$67B2 to add #N$19 points to the score.
 $6940 CALL $67B2    ; }
; Pick a random border colour using the Memory Refresh Register.
 $6943 LD A,R        ; {#REGl=the contents of the Memory Refresh Register.
 $6945 LD L,A        ; }
 $6946 LD H,$00      ; Set the low byte in #REGh to #N$00 so only low memory is
                     ; accessed.
 $6948 LD A,(HL)     ; #REGa=*#REGhl.
 $6949 LD ($5FB4),A  ; Write #REGa to *#R$5FB4.
 $694C AND %00111000 ; Keep only bits 3-5.
 $694E LD ($5C48),A  ; #HTML(Write #REGa to *<a rel="noopener nofollow"
                     ; href="https://skoolkit.ca/disassemblies/rom/hex/asm/5C48.html">BORDCR</a>.)
; Play a beep at the current pitch.
 $6951 POP DE        ; {Restore pitch adjustment and pitch from the stack.
 $6952 POP HL        ; }
 $6953 PUSH HL       ; {Stash the pitch and pitch adjustment on the stack.
 $6954 PUSH DE       ; }
 $6955 CALL $03B5    ; #HTML(Call <a rel="noopener nofollow"
                     ; href="https://skoolkid.github.io/rom/asm/03B5.html">BEEP</a>.)
 $6958 POP DE        ; {Restore the pitch adjustment and pitch from the stack.
 $6959 POP HL        ; }
; Lower the pitch for the next step by subtracting the pitch adjustment.
 $695A LD BC,$000F   ; #REGbc=#N($000F,$04,$04).
 $695D SBC HL,DE     ; #REGhl-=#REGde (with carry).
 $695F POP BC        ; Restore the step counter from the stack.
 $6960 RET           ; Return.

