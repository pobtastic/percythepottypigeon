; Put the stack pointer somewhere safe.
 $5DC2 LD SP,$5BFE     ; #REGsp=#R$5BFE.
 $5DC5 LD ($5FBC),A    ; {Write #N$00 to; #LIST
 $5DC8 LD ($5FAA),A    ; { *#R$5FBC }
                       ; { *#R$5FAA }
                       ; LIST#}
 $5DCB OUT ($FE),A     ; Set border to: #INK$00.
; #HTML(Open channel #N$02 (upper screen) via <a rel="noopener nofollow"
; href="https://skoolkit.ca/disassemblies/rom/hex/asm/1601.html">CHAN_OPEN</a>.)
 $5DCD CALL $1601      ; #HTML(Call <a rel="noopener nofollow"
                       ; href="https://skoolkit.ca/disassemblies/rom/hex/asm/1601.html">CHAN_OPEN</a>.)
; Clear the screen.
 $5DD0 LD HL,$5AFF     ; {Clear the screen and attribute buffers by filling #N$1AFF bytes
 $5DD3 LD DE,$5AFE     ; from #N$5AFE downwards with #N$00.
 $5DD6 LD (HL),$00     ;
 $5DD8 LD BC,$1AFF     ;
 $5DDB LDDR            ; }
; #HTML(Sets the border colour to use when calling
; <a rel="noopener nofollow" href="https://skoolkit.ca/disassemblies/rom/hex/asm/0D6B.html">CLS</a>.)
 $5DDD LD A,$0F        ; {#HTML(Write #INK$0F to
 $5DDF LD ($5C48),A    ; *<a rel="noopener nofollow" href="https://skoolkit.ca/disassemblies/rom/hex/asm/5C48.html">BORDCR</a>.)
                       ; }
 $5DE2 LD IX,$DAC0     ; #REGix=#R$DAC0.
 $5DE6 LD (IX+$23),$00 ; Write #N$00 to *#REGix+#N$23.
 $5DEA LD (IX+$02),$07 ; Write #N$07 to *#REGix+#N$02.
 $5DEE LD (IX+$22),$FF ; Write #N$FF to *#REGix+#N$22.
 $5DF2 CALL $653D      ; Call #R$653D.
 $5DF5 LD A,$FF        ; {Write #N$FF to *#R$5FBC.
 $5DF7 LD ($5FBC),A    ; }
; Game pause input loop.
@label=Pause_InputLoop
*$5DFA CALL $5FE9      ; Call #R$5FE9.
 $5DFD LD A,$FD        ; {Read from the keyboard;
 $5DFF IN A,($FE)      ; #TABLE(default,centre,centre,centre,centre,centre,centre)
                       ; { =h,r2 Port Number | =h,c5 Bit }
                       ; { =h 0 | =h 1 | =h 2 | =h 3 | =h 4 }
                       ; { #N$FD | A | S | D | F | G }
                       ; TABLE#}
 $5E01 AND %00000010   ; Keep only the bit which relates to the "S" key (bit
                       ; 1).
 $5E03 CALL Z,$6966    ; Call #R$6966 if the "S" key was pressed.
 $5E06 CALL $6992      ; Call #R$6992.
 $5E09 NOP             ; {No operation.
 $5E0A NOP             ; }
 $5E0B XOR A           ; {Write #N$00 to; #LIST
 $5E0C LD ($5FBB),A    ; { *#R$5FBB }
 $5E0F LD ($5FBD),A    ; { *#R$5FBD }
                       ; LIST#}
 $5E12 LD BC,$7FFE     ; {Read from the keyboard;
 $5E15 IN A,(C)        ; #TABLE(default,centre,centre,centre,centre,centre,centre)
                       ; { =h,r2 Port Number | =h,c5 Bit }
                       ; { =h 0 | =h 1 | =h 2 | =h 3 | =h 4 }
                       ; { #N$7F | SPACE | FULL-STOP | M | N | B }
                       ; TABLE#}
 $5E17 RRA             ; {Jump back to #R$5DFA if "SPACE" was not pressed.
 $5E18 JR C,$5DFA      ; }
; The "SPACE" key has been pressed.
 $5E1A LD B,$FE        ; {Read from the keyboard;
 $5E1C IN A,(C)        ; #TABLE(default,centre,centre,centre,centre,centre,centre)
                       ; { =h,r2 Port Number | =h,c5 Bit }
                       ; { =h 0 | =h 1 | =h 2 | =h 3 | =h 4 }
                       ; { #N$FE | SHIFT | Z | X | C | V }
                       ; TABLE#}
 $5E1E RRA             ; {Jump back to #R$5DFA if "CAPS SHIFT" was not pressed.
 $5E1F JR C,$5DFA      ; }
; Both "SPACE" and "CAPS SHIFT" were held down, so reset the game.
 $5E21 EI              ; Enable interrupts.
 $5E22 JR $5DC0        ; Jump to #R$5DC0.

; Populate Room Buffer
;
; Used by the routines at #R$5FE9, #R$655C, #R$659B and #R$68B8.
; .
; Fetch the current room number and set up the default tile and attribute
; data pointers.
@label=PopulateRoomBuffer
c$5E24 LD A,($5FC5)  ; Load #REGa with *#R$5FC5.
; Initialise the room pointer/ default tile set.
 $5E27 LD HL,$83A9   ; {Write #R$83A9 to *#R$5FC3.
 $5E2A LD ($5FC3),HL ; }
 $5E2D LD HL,$9BAA   ; {Write #R$9BAA to *#R$5FC1.
 $5E30 LD ($5FC1),HL ; }
; If this is not room #N$00, look up the correct room data pointer.
 $5E33 OR A          ; {Call #R$5FC6 if this is not room #N$00.
 $5E34 CALL NZ,$5FC6 ; }
