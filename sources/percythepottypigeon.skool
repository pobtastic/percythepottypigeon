; Draw Room Rows
;
; Used by the routine at #R$5F35.
; .
; Loop B times: call #R$6456 (draw row), advance #REGhl. Then call #R$63BB
; if header not drawn, and #R$63CB to colour bottom rows.
;
; B Number of rows to draw
; HL Buffer to use
@label=DrawRows_Room
c$6438 PUSH BC       ; {Stash the row counter and room buffer on the stack.
 $6439 PUSH HL       ; }
 $643A CALL $6456    ; Call #R$6456.
 $643D POP HL        ; {Restore the room buffer and row countr from the stack.
 $643E POP BC        ; }
; Advance the room buffer pointer to the next character row. Each row is
; #N$20 bytes wide; if adding #N$20 to #REGl overflows, carry into #REGh by
; adding #N$08 (into the next screen third).
 $643F LD A,$20      ; {#REGl+=#N$20.
 $6441 ADD A,L       ;
 $6442 LD L,A        ; }
 $6443 OR A          ; {Jump to #R$644A if there was no overflow.
 $6444 JR NZ,$644A   ; }
 $6446 LD A,$08      ; {#REGh+=#N$08.
 $6448 ADD A,H       ;
 $6449 LD H,A        ; }
@label=DrawRoomRows_Next
*$644A DJNZ $6438    ; Decrease the row counter by one and loop back to #R$6438 until all
                     ; of the rows have been drawn to the screen buffer.
 $644C LD A,($5FA1)  ; {Call #R$63BB if *#R$5FA1 is zero.
 $644F OR A          ;
 $6450 CALL Z,$63BB  ; }
 $6453 JP $63CB      ; Jump to #R$63CB.

; Draw Room Row
;
; Used by the routine at #R$6438.
;
; HL Pointer to the room buffer row
@label=DrawRow_Room
c$6456 LD B,$08      ; Set a counter in #REGb for #N$08 pixel lines.
 $6458 PUSH HL       ; Stash the room buffer pointer on the stack.
@label=DrawRoomRow_PixelLoop
*$6459 PUSH BC       ; {Stash the line counter and room buffer pointer on the
 $645A PUSH HL       ; stack.}
 $645B LD D,H        ; {Copy the room buffer pointer to #REGde.
 $645C LD E,L        ; }
 $645D RES 7,D       ; Reset bit 7 of #REGd to convert from a room buffer address to a
                     ; screen buffer address.
 $645F LD BC,$0020   ; {Copy #N($0020,$04,$04) bytes of data from the room buffer to the
 $6462 LDIR          ; screen buffer.}
 $6464 POP HL        ; {Restore the room buffer pointer and line counter from
 $6465 POP BC        ; the stack.}
 $6466 INC H         ; Move down one pixel line in the room buffer.
 $6467 DJNZ $6459    ; Decrease the line counter by one and loop back to #R$6459 until all
                     ; #N$08 pixel lines are copied.
 $6469 POP HL        ; Restore the original room buffer pointer from the stack.
 $646A LD A,H        ; #REGa=#REGh.
 $646B RRCA          ; {Rotate right three positions to extract the character
 $646C RRCA          ; row.
 $646D RRCA          ; }
 $646E AND %00000011 ; Keep only bits 0-1.
 $6470 OR %11011000  ; Set bits 3-4, 6-7 to form the attribute buffer high
                     ; byte.
 $6472 LD H,A        ; #REGh=#REGa.
 $6473 LD E,L        ; #REGe=#REGl.
 $6474 LD A,H        ; #REGa=#REGh.
 $6475 AND %00000011 ; Keep only bits 0-1.
 $6477 OR %01011000  ; Set bits 3-4, 6 to form the destination attribute buffer high
                     ; byte.
 $6479 LD D,A        ; #REGd=#REGa.
 $647A LD BC,$0020   ; {Copy #N($0020,$04,$04) bytes to the attribute buffer.
 $647D LDIR          ; }
 $647F RET           ; Return.

; Update Energy Bar
;
; Used by the routine at #R$5FE9.
; .
; Animates Percy's energy bar at the bottom of the screen. The bar
; depletes when Percy is airborne and refills when Percy is on the ground. If
; the bar fully depletes, Percy enters the falling state.
@label=UpdateEnergyBar
c$6480 LD HL,$52EC   ; #REGhl=#N$52EC (screen buffer location for the energy
                     ; bar).
; If *#R$5FA7 is non-zero, it's acting as a cooldown timer so decrement
; it and return.
 $6483 LD A,($5FA7)  ; #REGa=*#R$5FA7.
 $6486 OR A          ; {Jump to #R$648E if *#R$5FA7 is zero (Percy isn't
 $6487 JR Z,$648E    ; falling).}
 $6489 DEC A         ; Decrement the cooldown timer.
 $648A LD ($5FA7),A  ; Write #REGa to *#R$5FA7.
 $648D RET           ; Return.
; Check if Percy is on the ground (Y >= #N$90). If so, the energy bar
; .
; refills. Otherwise it depletes.
@label=UpdateEnergyBar_CheckPosition
*$648E LD A,(IX+$01) ; Load #REGa with Percy's Y position (from *#REGix+#N$01).
 $6491 CP $90        ; {Jump to #R$64BE if Percy is at, or below the ground
 $6493 JR NC,$64BE   ; level (#N$90).}
; Percy is airborne so deplete the energy bar. Uses a slower frame delay
; of #N$07 frames between each step.
 $6495 LD A,($5FAB)  ; #REGa=*#R$5FAB.
 $6498 INC A         ; Increment the frame delay counter.
 $6499 LD ($5FAB),A  ; Write #REGa back to *#R$5FAB.
 $649C CP $07        ; {Return if it's not yet time to update the energy bar.
 $649E RET NZ        ; }
; Reset the frame counter and check if Percy is on a platform (energy
; doesn't deplete while landed).
 $649F XOR A         ; {Write #N$00 to *#R$5FAB.
 $64A0 LD ($5FAB),A  ; }
 $64A3 LD A,($5FAD)  ; {Return if *#R$5FAD is set, indicating Percy is landed
 $64A6 OR A          ; on a platform.
 $64A7 RET NZ        ; }
; Deplete the energy bar by shifting pixels left. Scans rightward from
; the current position to find the rightmost filled byte, then shifts it left
; by one pixel.
@label=UpdateEnergyBar_Deplete_FindByte
*$64A8 LD A,(HL)     ; {Jump to #R$64B6 if *#REGhl is zero (no pixels here).
 $64A9 OR A          ;
 $64AA JR Z,$64B6    ; }
 $64AC SLA (HL)      ; Shift *#REGhl left one pixel (remove one pixel of
                     ; energy).
; Copy the updated byte to the three pixel rows below to give the
; energy bar its height.
@label=UpdateEnergyBar_CopyRows
*$64AE LD A,(HL)     ; #REGa=*#REGhl.
 $64AF INC H         ; {Copy to the first row below.
 $64B0 LD (HL),A     ; }
 $64B1 INC H         ; {Copy to the second row below.
 $64B2 LD (HL),A     ; }
 $64B3 INC H         ; {Copy to the third row below.
 $64B4 LD (HL),A     ; }
 $64B5 RET           ; Return.
; Current byte is empty so scan leftward for the next filled byte.
@label=UpdateEnergyBar_Deplete_ScanLeft
*$64B6 DEC L         ; Move one byte to the left.
 $64B7 LD A,L        ; {Jump to #R$64E1 if the energy bar is fully depleted (reached #N$E0,
 $64B8 CP $E0        ; the left edge of the bar).
 $64BA JR Z,$64E1    ; }
 $64BC JR $64A8      ; Jump to #R$64A8 to check this byte.
; This entry point is used by the routine at #R$69A7.
; .
; Percy is on the ground so refill the energy bar. Uses a faster frame
; delay of #N$03 frames between each step.
@label=UpdateEnergyBar_Refill
*$64BE LD A,($5FAB)  ; #REGa=*#R$5FAB.
 $64C1 INC A         ; Increment the frame delay counter.
 $64C2 LD ($5FAB),A  ; Write #REGa back to *#R$5FAB.
 $64C5 CP $03        ; {Return if not yet time to update the bar.
 $64C7 RET C         ; }
 $64C8 XOR A         ; {Write #N$00 to *#R$5FAB.
 $64C9 LD ($5FAB),A  ; }
; This entry point is used by the routine at #R$655C.
; .
; Refill the energy bar by shifting pixels right (filling from the left).
; Scans leftward to find the first non-full byte and fills it one pixel at a
; time.
@label=UpdateEnergyBar_Refill_Shift
*$64CC LD HL,$52E1   ; #REGhl=#N$52E1 (left edge of the energy bar).
@label=UpdateEnergyBar_Refill_FindByte
*$64CF LD A,(HL)     ; #REGa=*#REGhl.
 $64D0 CP $FF        ; {Jump to #R$64DA if this byte is full (scan next byte).
 $64D2 JR Z,$64DA    ; }
 $64D4 SRL (HL)      ; Shift *#REGhl right one pixel.
 $64D6 SET 7,(HL)    ; Set bit 7 of *#REGhl (fill from the left).
 $64D8 JR $64AE      ; Jump to #R$64AE to copy the result to the rows below.
; Current byte is fully filled so scan rightward for the next byte to
; fill.
@label=UpdateEnergyBar_Refill_ScanRight
*$64DA INC L         ; Move one byte to the right.
 $64DB LD A,L        ; {Return if the bar is fully refilled (the scan reached #N$ED which
 $64DC CP $ED        ; is the right edge of the bar).
 $64DE RET Z         ; }
 $64DF JR $64CF      ; Jump to #R$64CF to check this byte.
; Energy bar has fully depleted so set Percy to the falling state.
@label=UpdateEnergyBar_Depleted
*$64E1 LD A,$FF      ; {Write #N$FF to *#R$5FA7.
 $64E3 LD ($5FA7),A  ; }
 $64E6 RET           ; Return.

