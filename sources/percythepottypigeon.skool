 $624C LD A,(IX+$01)   ; {Jump to #R$626F if Percy's Y position (*#REGix+#N$01) is greater
 $624F CP $91          ; than or equal to #N$91 and Percy is at the bottom of the screen.
 $6251 JR NC,$626F     ; }
