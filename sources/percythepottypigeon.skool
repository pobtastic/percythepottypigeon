; character to draw.
;
; DE Tile counter
; HL Pointer to the room data
@label=Command08+_SingleTile
c$5EC8 LD A,(HL)     ; Fetch the tile ID from *#REGhl and store it in #REGa.
 $5EC9 INC HL        ; Increment the room data pointer by one to move past the command
                     ; byte.
 $5ECA PUSH HL       ; {Stash the room data pointer and tile counter on the
 $5ECB PUSH DE       ; stack.}
 $5ECC LD D,$00      ; #REGd=#N$00 (flag: draw tile to screen buffer).
 $5ECE CALL $5F35    ; Call #R$5F35.
 $5ED1 POP DE        ; {Restore the tile counter and room data pointer from the
 $5ED2 POP HL        ; stack.}
 $5ED3 DEC DE        ; Decrease #REGde by one.
 $5ED4 LD A,D        ; {Jump back to #R$5E6D if #REGde is zero.
 $5ED5 OR E          ;
 $5ED6 JR Z,$5E6D    ; }
