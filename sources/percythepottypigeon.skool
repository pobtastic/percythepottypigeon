 $6A6A LD IX,$DAC4     ; #REGix=#R$DAC4.
; Only check collision if the red bird is in the same room as the egg.
 $6A6E LD A,($6CB6)    ; {#REGb=*#R$6CB6.
 $6A71 LD B,A          ; }
 $6A72 LD A,($5FC5)    ; #REGa=*#R$5FC5.
 $6A75 CP B            ; {Jump to #R$6A9D if the red bird is not in the current
 $6A76 JR NZ,$6A9D     ; room.}
