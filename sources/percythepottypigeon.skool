 $6AEE DEC C           ;
 $6AEF DEC C           ;
 $6AF0 DEC C           ;
 $6AF1 DEC C           ;
 $6AF2 DEC C           ; }
 $6AF3 JR $6AD0        ; Jump to #R$6AD0 to try again.
 $6AF5 RET             ; Return.

; Update Red Bird Movement
;
; Used by the routine at #R$69F7.
; .
; Updates the red bird's position and animation. Handles the red bird's
; movement along its flight path, stun recovery, room transitions, and wing
; animation.
@label=UpdateRedBirdMovement
c$6AF6 LD IX,$DAC4     ; #REGix=#R$DAC4 (red bird sprite data).
; Reset the red bird's frame and colour to defaults.
 $6AFA LD (IX+$03),$00 ; Write #N$00 to *#REGix+#N$03 (clear frame/ set to
                       ; invisible).
 $6AFE LD (IX+$02),$02 ; Write #INK$02 to *#REGix+#N$02.
; If the red bird is stunned, handle stun recovery instead.
 $6B02 LD A,($6CB4)    ; {Jump to #R$6B9B if *#R$6CB4 is set indicating the red bird is
 $6B05 OR A            ; stunned.
 $6B06 JP NZ,$6B9B     ; }
; If the room is being initialised, set a random appearance delay.
 $6B09 LD A,($5FBB)    ; {Jump to #R$6B19 if *#R$5FBB is unset.
 $6B0C OR A            ;
 $6B0D JR Z,$6B19      ; }
 $6B0F CALL $6C39      ; Call #R$6C39.
 $6B12 OR %00000001    ; Set bit 0 (ensure an odd value).
 $6B14 AND %00111111   ; Keep only bits 0-5 (cap at #N$3F).
 $6B16 LD ($6CBD),A    ; Write #REGa to *#R$6CBD.
; If an appearance delay is active, the red bird is waiting to enter the
; current room so count down and handle the transition.
@label=UpdateRedBirdMovement_CheckDelay
*$6B19 LD A,($6CBD)    ; {Jump to #R$6B6E if *#R$6CBD is active.
 $6B1C OR A            ;
 $6B1D JR NZ,$6B6E     ; }
; Red bird is active in the current room so update its flight path.
 $6B1F LD A,($5FC5)    ; {Write *#R$5FC5 to *#R$6CB6 (store the red bird's
 $6B22 LD ($6CB6),A    ; current room).}
; Decrement the direction change timer.
 $6B25 LD HL,$6CB2     ; #REGhl=#R$6CB2.
 $6B28 DEC (HL)        ; Decrement *#REGhl.
 $6B29 JR NZ,$6B4A     ; Jump to #R$6B4A if the timer hasn't expired.
; Timer expired so choose a new flight direction.
 $6B2B XOR A           ; {Write #N$00 to *#R$6CBA.
 $6B2C LD ($6CBA),A    ; }
*$6B2F LD HL,$6CBA     ; #REGhl=#R$6CBA.
 $6B32 DEC (HL)        ; Decrement *#REGhl.
 $6B33 CALL Z,$6AA1    ; Call #R$6AA1 if *#REGhl reached zero (find a new
                       ; flight path).
