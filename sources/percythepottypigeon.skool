 $7193 OR A          ;
 $7194 RET Z         ; }
; Check the first frog.
 $7195 LD HL,$71E9   ; #REGhl=#R$71E9.
 $7198 LD IY,$DAE0   ; #REGiy=#R$DAE0.
 $719C LD IX,$DAC8   ; #REGix=#R$DAC8.
 $71A0 CALL $6C85    ; Call #R$6C85.
 $71A3 JR C,$71B9    ; Jump to #R$71B9 if the egg isn't colliding with the
                     ; first frog.
; Item hit the first frog.
 $71A5 LD A,(HL)     ; {Jump to #R$71B6 if the frog is already stunned.
 $71A6 OR A          ;
 $71A7 JR NZ,$71B6   ; }
; Stun the frog and set a random stun timer/ award points.
 $71A9 LD A,R        ; #REGa=the contents of the Memory Refresh Register.
 $71AB OR %01000000  ; Set bit 6 (ensure a minimum stun duration).
 $71AD LD (HL),A     ; Write the stun duration to the first frogs stun
                     ; countdown.
 $71AE LD A,$0A      ; {Call #R$67B2 to add #N$0A points to the score.
 $71B0 CALL $67B2    ; }
 $71B3 CALL $6F7A    ; Call #R$6F7A.
