 $6261 JR Z,$626C      ; }
 $6263 OR %11111111    ; {Write #N$FF to *#R$5FA7.
 $6265 LD ($5FA7),A    ; }
 $6268 LD (IX+$02),$FF ; Write #N$FF to *#REGix+#N$02.
@label=MainGameLoop_FallImpact_Done
