 $6116 LD A,($5FA7)    ; {Jump to #R$6152 if *#R$5FA7 is unset.
 $6119 OR A            ;
 $611A JR Z,$6152      ; }
 $611C CP $FE          ; {Jump to #R$612B if #REGa is #N$FE.
 $611E JR Z,$612B      ; }
; Start falling and set the fall state and initial beep pitch.
 $6120 LD A,$FE        ; {Write #N$FE to *#R$5FA7.
 $6122 LD ($5FA7),A    ; }
 $6125 LD HL,$0082     ; {Write #N$0082 to *#R$5FB7.
 $6128 LD ($5FB7),HL   ; }
; Percy is falling so play a descending beep and move downward.
@label=MainGameLoop_Falling
