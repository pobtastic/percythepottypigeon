; Play Next Level Jingle
;
; Used by the routine at #R$FC52.
;
; .
;
; #HTML(#AUDIO(next-level.wav)(#INCLUDE(NextLevel)))
@label=PlaySound_NextLevelJingle
c$FAC1 LD HL,$FAF9   ; Load #REGhl with a pointer to #R$FAF9.
; This entry point is used by the routine at #R$68B8.
@label=PlaySound
*$FAC4 NOP           ; No operation.
@label=PlaySound_Loop
*$FAC5 LD A,(HL)     ; Fetch the music data from *#REGhl.
 $FAC6 CP $FF        ; {Return if this is the terminator (#N$FF).
 $FAC8 RET Z         ; }
 $FAC9 OR A          ; {Jump to #R$FAEA if this is a rest.
 $FACA JR Z,$FAEA    ; }
 $FACC XOR A         ; {Set the speaker port to off.
 $FACD OUT ($FE),A   ; }
 $FACF LD E,(HL)     ; {Load the pitch period into both #REGe (reset value) and #REGd
 $FAD0 LD D,E        ; (countdown).}
 $FAD1 INC HL        ; {Load the note duration into #REGc.
 $FAD2 LD C,(HL)     ; }
@label=PlaySound_NoteLoop
*$FAD3 OUT ($FE),A   ; Send to the speaker to play the note.
 $FAD5 DEC D         ; Decrease the period counter by one.
 $FAD6 JR NZ,$FADB   ; Jump to #R$FADB if the period hasn't elapsed yet.
 $FAD8 LD D,E        ; Reset the period counter from #REGe.
 $FAD9 XOR %00011000 ; Toggle the speaker and MIC outputs.
@label=PlaySound_NoteInnerLoop
*$FADB DJNZ $FAD3    ; Decrease counter by one and loop back to #R$FAD3 until
                     ; counter is zero.
 $FADD DEC C         ; Decrease the duration counter.
 $FADE JR NZ,$FAD3   ; Jump back to #R$FAD3 until the full duration has
                     ; elapsed.
 $FAE0 INC HL        ; Advance to the next sound data entry.
 $FAE1 LD A,$BF      ; {Read from the keyboard;
 $FAE3 IN A,($FE)    ; #TABLE(default,centre,centre,centre,centre,centre,centre)
                     ; { =h,r2 Port Number | =h,c5 Bit }
                     ; { =h 0 | =h 1 | =h 2 | =h 3 | =h 4 }
                     ; { #N$BF | ENTER | L | K | J | H }
                     ; TABLE#}
 $FAE5 AND %00000001 ; {Return if "ENTER" has been pressed.
 $FAE7 RET Z         ; }
 $FAE8 JR $FAC5      ; Jump back to #R$FAC5.
@label=PlaySound_Rest
*$FAEA INC HL        ; Increment #REGhl by one.
 $FAEB LD C,(HL)     ; #REGc=*#REGhl.
@label=PlaySound_RestOuterLoop
*$FAEC NOP           ; No operation.
@label=PlaySound_RestInnerLoop
*$FAED NOP           ; No operation.
 $FAEE NOP           ; No operation.
 $FAEF NOP           ; No operation.
 $FAF0 NOP           ; No operation.
 $FAF1 DJNZ $FAED    ; Decrease the counter by one and loop back to #R$FAED
                     ; until counter is zero.
 $FAF3 DEC C         ; Decrease the rest duration counter.
 $FAF4 JR NZ,$FAEC   ; Jump back to #R$FAEC until the full rest duration has
                     ; elapsed.
 $FAF6 INC HL        ; Advance to the next sound data entry.
 $FAF7 JR $FAC5      ; Jump back to #R$FAC5.

