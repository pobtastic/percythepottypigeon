 $6AAD LD A,($6CB3)    ; #REGa=*#R$6CB3.
 $6AB0 XOR %00000001   ; Toggle bit 0 (flip direction).
 $6AB2 LD ($6CB3),A    ; Write #REGa to *#R$6CB3.
; Look up the flight speed for the current level from the table at
; #R$6CBE.
@label=HandleRedBird_LookupSpeed
*$6AB5 LD HL,$6CBE     ; #REGhl=#R$6CBE.
 $6AB8 LD A,($5FB1)    ; #REGa=*#R$5FB1.
 $6ABB DEC A           ; Decrement to make zero-indexed.
 $6ABC LD E,A          ; #REGe=#REGa.
 $6ABD LD D,$00        ; #REGd=#N$00.
 $6ABF ADD HL,DE       ; #REGhl+=#REGde.
 $6AC0 LD A,(HL)       ; #REGa=*#REGhl (flight speed for this level).
 $6AC1 LD ($6CB9),A    ; Write #REGa to *#R$6CB9.
