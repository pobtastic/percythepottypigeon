 $6155 LD B,(IX+$01)   ; #REGb=*#REGix+#N$01 (Percy's Y position).
 $6158 PUSH BC         ; Stash Percy's position on the stack.
 $6159 LD A,($5FA2)    ; {#REGe=*#R$5FA2.
 $615C LD E,A          ; }
; If no direction is pressed, decelerate.
 $615D CP $1F          ; {Call #R$62F8 if no direction has been pressed.
 $615F CALL Z,$62F8    ; }
 $6162 LD A,($5FAE)    ; {#REGd=*#R$5FAE.
 $6165 LD D,A          ; }
; Dispatch to each direction handler if active.
 $6166 BIT 4,E         ; {Call #R$6280 if left is active.
 $6168 CALL Z,$6280    ; }
 $616B BIT 3,E         ; {Call #R$62A2 if right is active.
 $616D CALL Z,$62A2    ; }
 $6170 BIT 1,E         ; {Call #R$62C2 if up is active.
 $6172 CALL Z,$62C2    ; }
 $6175 BIT 2,E         ; {Call #R$62D0 if down is active.
 $6177 CALL Z,$62D0    ; }
