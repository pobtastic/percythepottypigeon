 $6189 LD A,$00        ; {Write #N$00 to *#R$5FA8.
 $618B LD ($5FA8),A    ; }
 $618E LD E,$07        ; #REGe=#N$07.
; Load Percy's current position.
 $6190 LD C,(IX+$00)   ; #REGc=*#REGix+#N$00 (Percy's X position).
 $6193 LD B,(IX+$01)   ; #REGb=*#REGix+#N$01 (Percy's Y position).
