 $6ADD LD B,$04        ; #REGb=#N$04 (reset Y to the top).
 $6ADF LD A,($6CB3)    ; #REGa=*#R$6CB3 (flight direction).
 $6AE2 OR A            ; {Jump to #R$6AED if flying right.
 $6AE3 JR NZ,$6AED     ; }
