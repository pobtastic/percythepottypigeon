 $71BF LD IX,$DACC   ; #REGix=#R$DACC.
 $71C3 LD HL,$71EC   ; #REGhl=#R$71EC.
 $71C6 CALL $6C85    ; Call #R$6C85.
 $71C9 RET C         ; Return if the egg isn't colliding with the second frog.
