c$6581 PUSH HL       ; Stash the screen buffer pointer on the stack.
 $6582 LD DE,($5C36) ; #HTML(#REGde=*<a rel="noopener nofollow"
                     ; href="https://skoolkit.ca/disassemblies/rom/hex/asm/5C36.html">CHARS</a>.)
; Font data starts at character #N$20 (ASCII "SPACE"), so this allows us
; to reference the data from a zero index.
 $6586 SUB $20       ; Subtract #N$20 from the ASCII character input.
; #HTML(<a rel="noopener nofollow" href="https://skoolkit.ca/disassemblies/rom/hex/asm/5C36.html">CHARS</a>
; points #N$0100 bytes before the actual font data, so incrementing #REGd adds
; #N$0100 to compensate.)
 $6588 INC D         ; Increment #REGd by one (add #N$0100 to #REGde).
 $6589 LD L,A        ; {Set the ASCII character to print in #REGhl.
 $658A LD H,$00      ; }
; Each ASCII character is #N$08 bytes of data, so this translates the
; character ID into the offset.
 $658C ADD HL,HL     ; {Multiply #REGhl by #N$08 and add the base font address.
 $658D ADD HL,HL     ;
 $658E ADD HL,HL     ;
 $658F ADD HL,DE     ; }
 $6590 EX DE,HL      ; #REGde now points to the character font data.
 $6591 POP HL        ; Restore the screen buffer pointer from the stack into
                     ; #REGhl.
 $6592 LD B,$08      ; Set a line counter in #REGb (#N$08 lines in a UDG).
@label=PrintCharacter_LineLoop
*$6594 LD A,(DE)     ; {Copy the UDG data to the screen buffer.
 $6595 LD (HL),A     ; }
 $6596 INC H         ; Move down one pixel line in the screen buffer.
 $6597 INC DE        ; Move to the next UDG graphic data byte.
 $6598 DJNZ $6594    ; Decrease the line counter by one and loop back to #R$6594 until all
                     ; #N$08 lines of the UDG character have been drawn.
 $659A RET           ; Return.

; Main Menu
;
; Used by the routine at #R$653D.
; .
; Displays the pre-game title screen.
;
; .
;
; Room #N$0C is the title page.
@label=MainMenu
c$659B LD A,$0C      ; {Write #N$0C to; #LIST
 $659D LD ($5FC5),A  ; { *#R$5FC5 }
 $65A0 LD ($5FA1),A  ; { *#R$5FA1 }
                     ; LIST#}
 $65A3 CALL $5E24    ; Call #R$5E24.
 $65A6 XOR A         ; {Write #N$00 to *#R$5FA1.
 $65A7 LD ($5FA1),A  ; }
 $65AA LD HL,$4896   ; #REGhl=#N$4896 (screen buffer location).
 $65AD LD B,$04      ; Set a counter in #REGb for #N$04 rows of text data.
 $65AF LD DE,$660D   ; Point #REGde to #R$660D.
*$65B2 PUSH BC       ; {Stash the row counter and screen buffer pointer on the
 $65B3 PUSH HL       ; stack.}
 $65B4 LD B,$05      ; Set a counter in #REGb for #N$05 characters to print in
                     ; each row.
*$65B6 PUSH HL       ; {Stash the screen buffer pointer, message pointer and the character
 $65B7 PUSH DE       ; counter on the stack.
 $65B8 PUSH BC       ; }
 $65B9 LD A,(DE)     ; Fetch a character from the message string and store it
                     ; in #REGa.
 $65BA CALL $6581    ; Call #R$6581.
 $65BD POP BC        ; {Restore the character counter, message pointer and the screen
 $65BE POP DE        ; buffer pointer from the stack.
 $65BF POP HL        ; }
 $65C0 INC L         ; Increment #REGl by one.
 $65C1 INC DE        ; Increment #REGde by one.
 $65C2 DJNZ $65B6    ; Decrease counter by one and loop back to #R$65B6 until
                     ; counter is zero.
 $65C4 POP HL        ; Restore #REGhl from the stack.
 $65C5 LD BC,$0020   ; {#REGhl+=#N($0020,$04,$04).
 $65C8 ADD HL,BC     ; }
 $65C9 POP BC        ; Restore #REGbc from the stack.
 $65CA DJNZ $65B2    ; Decrease counter by one and loop back to #R$65B2 until
                     ; counter is zero.
 $65CC LD B,$3D      ; #REGb=#N$3D.
 $65CE CALL $FC1B    ; Call #R$FC1B.
 $65D1 LD BC,$BFFE   ; {Read from the keyboard;
@label=WaitForEnterToStart_Loop
*$65D4 IN A,(C)      ; #TABLE(default,centre,centre,centre,centre,centre,centre)
                     ; { =h,r2 Port Number | =h,c5 Bit }
                     ; { =h 0 | =h 1 | =h 2 | =h 3 | =h 4 }
                     ; { #N$BF | ENTER | L | K | J | H }
                     ; TABLE#}
 $65D6 RRCA          ; {Jump to #R$65D4 if "ENTER" is not being pressed.
 $65D7 JR C,$65D4    ; }
 $65D9 LD A,$01      ; {Set the border to #INK$01.
 $65DB OUT ($FE),A   ; }
 $65DD LD B,$0A      ; #REGb=#N$0A.
 $65DF LD HL,$5FA1   ; #REGhl=#R$5FA1.
*$65E2 LD (HL),$00   ; Write #N$00 to *#REGhl.
 $65E4 INC HL        ; Increment #REGhl by one.
 $65E5 DJNZ $65E2    ; Decrease counter by one and loop back to #R$65E2 until
                     ; counter is zero.
 $65E7 JP $6562      ; Jump to #R$6562.

; Draw Chicks In The Nest
;
; Used by the routine at #R$6992.
@label=DrawNestChicks
c$65EA LD HL,$E08A   ; #REGhl=#R$E000(#N$E08A) (sprite buffer position for the first
                     ; chick).
 $65ED LD B,$03      ; Set a chick counter in #REGb for #N$03 chicks.
@label=DrawNestChicks_Loop
*$65EF PUSH BC       ; Stash the chick counter on the stack.
 $65F0 PUSH HL       ; Stash the screen position on the stack.
 $65F1 LD HL,$A763   ; #REGhl=#R$A763 (base address of the chick animation
                     ; frames).
 $65F4 LD BC,$0008   ; #REGbc=#N$0008 (bytes per frame).
; Use the rotating bit pattern at #R$5FB4 to select which animation frame
; to use for this chick. The pattern is rotated each time, so each chick picks
; up a different bit and animates independently.
 $65F7 LD A,($5FB4)  ; {Update *#R$5FB4 by rotating the frame selection bits.
 $65FA RRCA          ;
 $65FB LD ($5FB4),A  ; }
 $65FE JR C,$6601    ; Jump to #R$6601 if carry is set (use first frame at
                     ; #R$A763).
 $6600 ADD HL,BC     ; #REGhl+=#REGbc (use second frame at #R$A76B).
@label=DrawNestChicks_DrawFrame
*$6601 EX DE,HL      ; Transfer the frame graphic address to #REGde.
 $6602 POP HL        ; Restore the screen position from the stack.
 $6603 PUSH HL       ; Keep a copy of the screen position on the stack.
 $6604 CALL $6692    ; Call #R$6692 to draw the chick frame to the screen.
 $6607 POP HL        ; {Restore the screen position and chick counter from the
 $6608 POP BC        ; stack.}
 $6609 INC L         ; Move one character column to the right for the next
                     ; chick.
 $660A DJNZ $65EF    ; Decrease the chick counter by one and loop back to #R$65EF until all
                     ; #N$03 chicks are drawn.
 $660C RET           ; Return.

