 $5EFD JR Z,$5F21    ; }
 $5EFF CP $24        ; {Jump to #R$5F74 if #REGa is #N$24 (end of attribute
 $5F01 JR Z,$5F74    ; data).}
; Otherwise, write this byte directly as a single attribute value.
 $5F03 LD C,(HL)     ; #REGc=*#REGhl.
 $5F04 LD (IX+$00),C ; Write #REGc to *#REGix+#N$00.
 $5F07 DEC DE        ; Decrease #REGde by one.
 $5F08 INC IX        ; Increment #REGix by one.
 $5F0A LD A,E        ; {Jump back to #R$5E6D if #REGde is zero.
 $5F0B OR D          ;
 $5F0C JP Z,$5E6D    ; }
 $5F0F INC HL        ; Increment #REGhl by one.
 $5F10 JR $5EF6      ; Jump to #R$5EF6.
; Attribute overlay command #N$12: skip over a number of attribute cells.
; .
; The following byte gives the skip count.
@label=PopulateRoomBuffer_AttributeSkip
