 $5E37 LD HL,$C000   ; {Clear #N$17FF bytes from #R$C000 onwards.
 $5E3A LD DE,$C001   ;
 $5E3D LD (HL),$00   ;
 $5E3F LD BC,$17FF   ;
 $5E42 LDIR          ; }
; Initialise drawing state variables.
 $5E44 LD A,$FF      ; {Write #N$FF to *#R$5FBB.
 $5E46 LD ($5FBB),A  ; }
 $5E49 LD HL,$00FF   ; {Write #N($00FF,$04,$04) to *#R$5FB9.
 $5E4C LD ($5FB9),HL ; }
 $5E4F LD DE,$02C1   ; #REGde=#N$02C1.
; The main room data parsing loop. Reads a command byte from the room
; data and dispatches to the appropriate handler based on its value.
 $5E52 LD HL,($5FC3) ; Fetch the *#R$5FC3 and store it in #REGhl.
; This entry point is used by the routines at #R$5E72, #R$5E92, #R$5EAA and
; #R$5EC8.
@label=PopulateRoomBuffer_ParseByte
*$5E55 LD A,(HL)     ; Fetch the room data byte from the pointer.
 $5E56 OR A          ; {Jump to #R$5E6F if the room data byte is #N$00.
 $5E57 JR Z,$5E6F    ; }
 $5E59 CP $01        ; {Jump to #R$5E92 if the room data byte is #N$01.
 $5E5B JR Z,$5E92    ; }
 $5E5D CP $02        ; {Jump to #R$5EAA if the room data byte is #N$02.
 $5E5F JR Z,$5EAA    ; }
 $5E61 CP $03        ; {Jump to #R$5EDB if the room data byte is #N$03.
 $5E63 JR Z,$5EDB    ; }
 $5E65 CP $04        ; {Jump to #R$5E72 if the room data byte is #N$04.
 $5E67 JR Z,$5E72    ; }
 $5E69 CP $08        ; {Jump to #R$5EC8 if the room data byte is #N$08 or
 $5E6B JR NC,$5EC8   ; higher.}
; This entry point is used by the routines at #R$5E92, #R$5EAA, #R$5EC8 and
; #R$5EDB.
; .
; If none of the above matched, trigger an error.
@label=PopulateRoomBuffer_Error
*$5E6D RST $08       ; #HTML(Run the ERROR_1 routine:
                     ; <a rel="noopener nofollow" href="https://skoolkit.ca/disassemblies/rom/hex/asm/0008.html">RST #N$08</a>.)
 $5E6E INC B         ; Increment #REGb by one.
; Command #N$00: no-op. Skip this byte and continue parsing.
@label=Command00_Skip
*$5E6F INC HL        ; Increment the room data pointer by one.
 $5E70 JR $5E55      ; Jump to #R$5E55.

; Command #N$04: Switch Tile Set
;
; Used by the routine at #R$5E24.
; .
; Command #N$04: Switch the tile set.
; .
; The following byte selects which tile set to use â€” #N$01 for the default set
; at #R$9BAA, #N$02 for the alternate set at #R$A36A.
;
; DE Tile counter
; HL The room data pointer
@label=Command04_SwitchTileSet
c$5E72 INC HL        ; Increment the room data pointer by one.
 $5E73 LD A,(HL)     ; Fetch the tile set byte.
 $5E74 INC HL        ; Increment the room data pointer by one to move past the command
                     ; byte.
 $5E75 CP $01        ; {Jump to #R$5E88 if the tile set byte is equal to #N$01.
 $5E77 JR Z,$5E88    ; }
 $5E79 CP $02        ; {Jump to #R$5FE7 if the tile set byte is not equal to
 $5E7B JP NZ,$5FE7   ; #N$02.}
; Tile set #N$02: switch to the alternate tile set.
 $5E7E PUSH HL       ; Stash the room data pointer on the stack temporarily.
 $5E7F LD HL,$A36A   ; {Write #R$A36A to *#R$5FC1.
 $5E82 LD ($5FC1),HL ; }
 $5E85 POP HL        ; Restore the room data pointer from the stack.
 $5E86 JR $5E55      ; Jump back to #R$5E55 to continue parsing.
; Tile set #N$01: switch back to the default tile set.
@label=Set_DefaultTileSet
*$5E88 PUSH HL       ; Stash the room data pointer on the stack temporarily.
 $5E89 LD HL,$9BAA   ; {Write #R$9BAA to *#R$5FC1.
 $5E8C LD ($5FC1),HL ; }
 $5E8F POP HL        ; Restore the room data pointer from the stack.
 $5E90 JR $5E55      ; Jump back to #R$5E55 to continue parsing.

; Command #N$01: Draw Repeated Tile
;
; Used by the routine at #R$5E24.
; .
; Command #N$01: Draw a repeated tile. The following byte specifies how
; many times to repeat the current tile.
;
; DE Tile counter
; HL The room data pointer
@label=Command01_RepeatedTile
c$5E92 INC HL        ; Increment the room data pointer by one.
 $5E93 LD A,(HL)     ; Fetch the tile byte/ repeat counter.
 $5E94 INC HL        ; Increment the room data pointer by one to move past the command
                     ; byte.
 $5E95 LD B,A        ; Set the repeat counter in #REGb.
@label=RepeatedTile_Loop
*$5E96 PUSH HL       ; {Stash the room data pointer, tile counter and repeat counter on the
 $5E97 PUSH DE       ; stack.
 $5E98 PUSH BC       ; }
 $5E99 LD D,$01      ; #REGd=#N$01 (flag: advance position only, don't draw).
 $5E9B CALL $5F35    ; Call #R$5F35.
 $5E9E POP BC        ; {Restore the repeat counter, tile counter and room data pointer from
 $5E9F POP DE        ; the stack.
 $5EA0 POP HL        ; }
 $5EA1 DEC DE        ; Decrease the tile counter by one.
 $5EA2 LD A,E        ; {Jump to #R$5E6D if the tile counter is zero.
 $5EA3 OR D          ;
 $5EA4 JR Z,$5E6D    ; }
 $5EA6 DJNZ $5E96    ; Decrease the repeat counter by one and loop back to #R$5E96 until
                     ; all repeats are processed.
 $5EA8 JR $5E55      ; Jump to #R$5E55 to continue parsing.

; Command #N$02: Tile With Attribute
;
; Used by the routine at #R$5E24.
; .
; Command #N$02: Draw a tile with an explicit attribute.
; .
; The following two bytes specify the repeat count and the tile character to
; draw.
;
; DE Tile counter
; HL Pointer to the room data
@label=Command02_TileWithAttribute
c$5EAA INC HL        ; Increment the room data pointer by one.
 $5EAB LD A,(HL)     ; {Fetch the repeat counter from *#REGhl and store it in
 $5EAC LD B,A        ; #REGb.}
 $5EAD INC HL        ; Increment the room data pointer by one.
 $5EAE LD A,(HL)     ; Fetch the tile ID from *#REGhl and store it in #REGa.
 $5EAF INC HL        ; Increment the room data pointer by one to move past the command
                     ; bytes.
@label=TileWithAttribute_Loop
*$5EB0 PUSH AF       ; {Stash the tile character, repeat counter, tile counter and room data
 $5EB1 PUSH BC       ; pointer on the stack.
 $5EB2 PUSH DE       ;
 $5EB3 PUSH HL       ; }
 $5EB4 LD D,$00      ; #REGd=#N$00 (flag: draw tile to screen buffer).
 $5EB6 CALL $5F35    ; Call #R$5F35.
 $5EB9 POP HL        ; {Restore the room data pointer, tile counter, repeat counter and tile
 $5EBA POP DE        ; character from the stack.
 $5EBB POP BC        ;
 $5EBC POP AF        ; }
 $5EBD EX AF,AF'     ; Switch to shadow #REGaf to preserve the tile character.
 $5EBE DEC DE        ; Decrease the tile counter by one.
 $5EBF LD A,E        ; {Jump back to #R$5E6D if the tile counter is zero.
 $5EC0 OR D          ;
 $5EC1 JR Z,$5E6D    ; }
 $5EC3 EX AF,AF'     ; Switch back to main #REGaf to retrieve the tile
                     ; character.
 $5EC4 DJNZ $5EB0    ; Decrease the repeat counter by one and loop back to #R$5EB0 until
                     ; all tiles are drawn.
 $5EC6 JR $5E55      ; Jump to #R$5E55 to continue parsing.

